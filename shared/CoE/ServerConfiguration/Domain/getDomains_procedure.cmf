&#x9;&#x9;SET pos5 = INSTR(resourceEntry, &apos;&lt;user:name&gt;&apos;);
&#x9;&#x9;SET pos6 = INSTR(resourceEntry, &apos;&lt;/user:name&gt;&apos;);
