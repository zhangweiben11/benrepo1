&#x9;&#x9;&#x9;&#x9;SET rec.attributeName = TRIM(SUBSTRING(xmlEntry, pos5+len, pos6-(pos5+len)));
&#x9;&#x9;&#x9;&#x9;SET numFound = numFound + 1;
&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;-- Extract the type
&#x9;&#x9;&#x9;SET rec.attributeType = null;
