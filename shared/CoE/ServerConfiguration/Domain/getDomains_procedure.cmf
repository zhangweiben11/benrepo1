&#x9;&#x9;SET numEntries = 0;
&#x9;&#x9;SET numFound = 0;
&#x9;&#x9;SET pos3 = INSTR(resourceAttributesEntry, &apos;&lt;&apos;||namespaceTag2||&apos;:attribute&gt;&apos;);
