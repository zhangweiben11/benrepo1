&#x9;&#x9;END IF;

&#x9;&#x9;-- Extract the &quot;domainType&quot; name value
&#x9;&#x9;SET rec.domainType = null;
&#x9;&#x9;SET pos5 = INSTR(resourceEntry, &apos;&lt;user:domainType&gt;&apos;);
