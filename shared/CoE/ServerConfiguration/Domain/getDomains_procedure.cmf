&#x9;&#x9;&#x9;SET len = LENGTH(&apos;&lt;&apos;||namespaceTag2||&apos;:attribute&gt;&apos;);
&#x9;&#x9;&#x9;SET xmlEntry = &apos;    &apos;||TRIM(SUBSTRING(resourceAttributesEntry, pos3, pos4-pos3+len+1));
