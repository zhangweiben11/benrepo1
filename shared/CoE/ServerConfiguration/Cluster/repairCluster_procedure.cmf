&#x9;The details can be found in the file LICENSE.
&#x9;
&#x9;The following proprietary files are included as a convenience, and may not be used except pursuant
&#x9;to valid license to Composite Information Server or TIBCOÂ® Data Virtualization Server:
&#x9;csadmin-XXXX.jar, csarchive-XXXX.jar, csbase-XXXX.jar, csclient-XXXX.jar, cscommon-XXXX.jar,
&#x9;csext-XXXX.jar, csjdbc-XXXX.jar, csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar,
&#x9;and customproc-XXXX.jar (where -XXXX is an optional version number).  Any included third party files
&#x9;are licensed under the terms contained in their own accompanying LICENSE files, generally named .LICENSE.txt.
&#x9;
&#x9;This software is licensed AS-IS. Support for this software is not covered by standard maintenance agreements with TIBCO.
&#x9;If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
&#x9;agreement with TIBCO.
*/
PROCEDURE repairCluster(
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;-- Y=debug on.  N=debug off.
&#x9;IN repairOption&#x9;&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The option to perform the repair. 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;&quot;REGROUP&quot; - initiate a cluster regroup does not require the other input parameters.
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;&quot;REPAIR_FULL&quot; - a full repair of this node requires the other input parameters to send the repair request to.
&#x9;IN remoteHostName&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The remote host name to repair
&#x9;IN remotePort&#x9;&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The remote port to repair
&#x9;IN remoteUserName&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The remote user name to repair
&#x9;IN remoteDomainName&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The remote domain name to repair
&#x9;IN remotePassword&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The remote password to repair
&#x9;OUT result&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;-- SUCCESS or an exception is thrown
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;VARCHAR DEFAULT &apos;repairCluster&apos;;
&#x9;DECLARE debugPrintSize&#x9;&#x9;INTEGER DEFAULT 63900;
&#x9;DECLARE request&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE response&#x9;&#x9;&#x9;XML;
&#x9;DECLARE faultResponse&#x9;&#x9;XML;
&#x9;DECLARE errorMessage&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;SET result = &apos;SUCCESS&apos;;
&#x9;IF (repairOption IS NULL OR LENGTH(TRIM(repairOption)) = 0) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The &quot;repairOption&quot; may not be null or empty.&apos;;
&#x9;END IF;
&#x9;IF (UPPER(repairOption) NOT IN (&apos;REGROUP&apos;,&apos;REPAIR_FULL&apos;)) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The &quot;repairOption&quot; must be one of (&apos;&apos;REGROUP&apos;&apos;,&apos;&apos;REPAIR_FULL&apos;&apos;)&apos;;
&#x9;END IF;
&#x9;IF (UPPER(repairOption) = &apos;REPAIR_FULL&apos;) THEN
&#x9;&#x9;IF (remoteHostName IS NULL OR LENGTH(TRIM(remoteHostName)) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remoteHostName&quot; may not be null or empty.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (remotePort IS NULL OR remotePort = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remotePort&quot; may not be null or empty.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (remoteDomainName IS NULL OR LENGTH(TRIM(remoteDomainName)) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remoteDomainName&quot; may not be null or empty.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (remoteUserName IS NULL OR LENGTH(TRIM(remoteUserName)) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remoteUserName&quot; may not be null or empty.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (remotePassword IS NULL OR LENGTH(TRIM(remotePassword)) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remotePassword&quot; may not be null or empty.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (LOWER(TRIM(remoteHostName)) = &apos;localhost&apos;) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remoteHostName&quot; may not reference &quot;localhost&quot;.  Actual hostnames must be used.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;SET request = 
&#x9;&#x9;&#x9;&apos;&lt;server:repairCluster xmlns:server=&quot;http://www.compositesw.com/services/system/admin/server&quot;&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:option&gt;REPAIR_FULL&lt;/server:option&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:remoteHostName&gt;&apos;||CAST(XMLTEXT(remoteHostName) AS LONGVARCHAR)||&apos;&lt;/server:remoteHostName&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:remotePort&gt;&apos;||CAST(XMLTEXT(remotePort) AS LONGVARCHAR)||&apos;&lt;/server:remotePort&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:remoteDomainName&gt;&apos;||CAST(XMLTEXT(remoteDomainName) AS LONGVARCHAR)||&apos;&lt;/server:remoteDomainName&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:remoteUserName&gt;&apos;||CAST(XMLTEXT(remoteUserName) AS LONGVARCHAR)||&apos;&lt;/server:remoteUserName&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:remotePassword&gt;&apos;||CAST(XMLTEXT(remotePassword) AS LONGVARCHAR)||&apos;&lt;/server:remotePassword&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;&lt;/server:repairCluster&gt;&apos;;
&#x9;ELSE
&#x9;&#x9;SET request = 
&#x9;&#x9;&#x9;&apos;&lt;server:repairCluster xmlns:server=&quot;http://www.compositesw.com/services/system/admin/server&quot;&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;  &lt;server:option&gt;REGROUP&lt;/server:option&gt;&apos;||CHR(10)||
&#x9;&#x9;&#x9;&apos;&lt;/server:repairCluster&gt;&apos;;
&#x9;END IF;
&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;CALL PRINT(moduleName||&apos; : request=&apos;||CHR(10)||NVL(CAST(request AS LONGVARCHAR),&apos;null&apos;));
&#x9;END IF;

&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;-- Invoke the web service API to repair a node in a cluster.
&#x9;&#x9;CALL /services/webservices/system/admin/server/operations/repairCluster(request, response, faultResponse);
&#x9;&#x9;IF (faultResponse IS NOT NULL) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE CAST(faultResponse AS LONGVARCHAR);
&#x9;&#x9;END IF;
&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;CALL PRINT(moduleName||&apos; : response=&apos;||CHR(10)||NVL(CAST(response AS LONGVARCHAR),&apos;null&apos;));
&#x9;&#x9;END IF;
&#x9;EXCEPTION
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;SET errorMessage = CAST(CURRENT_EXCEPTION.TRACE AS LONGVARCHAR);
&#x9;&#x9;&#x9;CALL LOG(&apos;COEUtilities : EXCEPTION=[&apos;||errorMessage||&apos;]&apos;);
&#x9;&#x9;&#x9;RAISE ex VALUE errorMessage;
&#x9;END;
END</attribute>
