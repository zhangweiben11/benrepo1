PROCEDURE removeFromCluster(
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;-- Y=debug on.  N=debug off.
&#x9;IN remoteHostName&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The host name of the remote server to be removed.  Can be omitted if it is the invoking server.
&#x9;IN remotePort&#x9;&#x9;&#x9;&#x9;VARCHAR,&#x9;-- The port of the remote server to be removed.  Can be omitted if it is the invoking server, or it is the default port.
&#x9;OUT result&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;-- SUCCESS or an exception is thrown
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;VARCHAR DEFAULT &apos;removeFromCluster&apos;;
&#x9;DECLARE debugPrintSize&#x9;&#x9;INTEGER DEFAULT 63900;
&#x9;DECLARE request&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE response&#x9;&#x9;&#x9;XML;
&#x9;DECLARE faultResponse&#x9;&#x9;XML;
&#x9;DECLARE errorMessage&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;SET result = &apos;SUCCESS&apos;;
&#x9;IF (remoteHostName IS NOT NULL AND LOWER(TRIM(remoteHostName)) = &apos;localhost&apos;) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The &quot;remoteHostName&quot; may not reference &quot;localhost&quot;.  Actual hostnames must be used.&apos;;
&#x9;END IF;

&#x9;SET request = 
&#x9;&#x9;&apos;&lt;server:removeFromCluster xmlns:server=&quot;http://www.compositesw.com/services/system/admin/server&quot;&gt;&apos;||CHR(10);
&#x9;IF (remoteHostName IS NOT NULL) THEN
&#x9;&#x9;SET request = request ||
&#x9;&#x9;&apos;  &lt;server:remoteHostName&gt;&apos;||CAST(XMLTEXT(remoteHostName) AS LONGVARCHAR)||&apos;&lt;/server:remoteHostName&gt;&apos;||CHR(10);
&#x9;END IF;
&#x9;IF (remotePort IS NOT NULL) THEN
&#x9;&#x9;SET request = request ||
&#x9;&#x9;&apos;  &lt;server:remotePort&gt;&apos;||CAST(XMLTEXT(remotePort) AS LONGVARCHAR)||&apos;&lt;/server:remotePort&gt;&apos;||CHR(10);
&#x9;END IF;
&#x9;SET request = request ||
&#x9;&#x9;&apos;&lt;/server:removeFromCluster&gt;&apos;;
&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;CALL PRINT(moduleName||&apos; : request=&apos;||CHR(10)||NVL(CAST(request AS LONGVARCHAR),&apos;null&apos;));
&#x9;END IF;

&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;-- Invoke the web service API to remove a node from a cluster
&#x9;&#x9;CALL /services/webservices/system/admin/server/operations/removeFromCluster(request, response, faultResponse);
&#x9;&#x9;IF (faultResponse IS NOT NULL) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE CAST(faultResponse AS LONGVARCHAR);
&#x9;&#x9;END IF;
&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;CALL PRINT(moduleName||&apos; : response=&apos;||CHR(10)||NVL(CAST(response AS LONGVARCHAR),&apos;null&apos;));
&#x9;&#x9;END IF;
&#x9;EXCEPTION
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;SET errorMessage = CAST(CURRENT_EXCEPTION.TRACE AS LONGVARCHAR);
&#x9;&#x9;&#x9;CALL LOG(&apos;COEUtilities : EXCEPTION=[&apos;||errorMessage||&apos;]&apos;);
&#x9;&#x9;&#x9;RAISE ex VALUE errorMessage;
&#x9;END;
END</attribute>
