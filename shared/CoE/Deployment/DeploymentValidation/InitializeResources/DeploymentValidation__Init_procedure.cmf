PROCEDURE DeploymentValidation_Init(
&#x9;IN destroyResources&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=destroy resources.  N=do not destroy resources.
&#x9;IN datasourcePath&#x9;&#x9;&#x9;&#x9;VARCHAR(4000),&#x9;-- The path to the oracle, sqlserver or postgres datasource
&#x9;IN catalogName&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR,&#x9;&#x9;-- The catalog name if applicable otherwise null.
&#x9;IN schemaName&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR,&#x9;&#x9;-- The schema name if applicable otherwise null.
&#x9;OUT status&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR&#x9;&#x9;&#x9;-- SUCCESS or an exception is thrown.
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;&#x9;DEFAULT &apos;DeploymentValidation_Init&apos;;
&#x9;DECLARE datasourcePathMod&#x9;&#x9;&#x9;VARCHAR(4000) &#x9;DEFAULT REPLACE(datasourcePath, &apos;&quot;&apos;, &apos;&apos;);&#x9;&#x9;-- The path to the oracle, sqlserver or postgres datasource
&#x9;DECLARE resourceContainer&#x9;&#x9;&#x9;VARCHAR(4000)&#x9;DEFAULT &apos;/shared/CoE/DeploymentValidation&apos;;

&#x9;-- Invoke the initialization script
&#x9;CALL /shared/ASAssets/Utilities/deployment/validate/helpers/validateDeploymentInit(
&#x9;&#x9;destroyResources,&#x9;-- Y=destroy and recreate resources.  N=do not destroy resources.
&#x9;&#x9;datasourcePathMod,&#x9;-- The path to the oracle, sqlserver or postgres datasource
&#x9;&#x9;catalogName,&#x9;&#x9;-- The catalog name if applicable otherwise null.
&#x9;&#x9;schemaName,&#x9;&#x9;&#x9;-- The schema name if applicable otherwise null.
&#x9;&#x9;resourceContainer,&#x9;-- The resource container/folder which will contain the generated resources.  If the path does not exist, it will be created.
&#x9;&#x9;status&#x9;&#x9;&#x9;&#x9;-- SUCCESS or an exception is thrown.
&#x9;);
END</attribute>
