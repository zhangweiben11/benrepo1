  <attribute name="SQL" type="STRING">select count(*) cnt, 
&#x9;p.member_id, 
&#x9;d.domain_id, 
&#x9;m.name, 
&#x9;CAST(CASE
&#x9;&#x9;WHEN m.&quot;type&quot; = 15 then &apos;USER&apos;
&#x9;&#x9;WHEN m.&quot;type&quot; = 16 then &apos;GROUP&apos;
&#x9;&#x9;ELSE &apos;UNKOWN&apos;
&#x9;END AS VARCHAR(10)) AS name_type,
&#x9;d.domain_name, d.domain_type
from /shared/CoE/DVRepo/Datasources/DVPostgresRepo/cisrepo/security_privileges p
inner join /shared/CoE/DVRepo/Datasources/DVPostgresRepo/cisrepo/security_members m
  on p.member_id = m.id
inner join /shared/CoE/DVRepo/Datasources/DVPostgresRepo/cisrepo/system_domains d
  on m.domain_id = d.domain_id
group by p.member_id, 
&#x9;d.domain_id, 
&#x9;m.name, &#x9;
&#x9;CASE
&#x9;&#x9;WHEN m.&quot;type&quot; = 15 then &apos;USER&apos;
&#x9;&#x9;WHEN m.&quot;type&quot; = 16 then &apos;GROUP&apos;
&#x9;&#x9;ELSE &apos;UNKOWN&apos;
&#x9;END,
&#x9;d.domain_name, 
&#x9;d.domain_type
order by 1 desc, name</attribute>
