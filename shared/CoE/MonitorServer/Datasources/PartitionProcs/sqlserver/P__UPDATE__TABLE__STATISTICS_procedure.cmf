PROCEDURE P_UPDATE_TABLE_STATISTICS(
&#x9;IN debug&#x9;&#x9;&#x9;CHAR(1),
&#x9;IN schemaName&#x9;&#x9;VARCHAR,
&#x9;IN tableName&#x9;&#x9;VARCHAR,
&#x9;OUT success&#x9;&#x9;&#x9;INTEGER
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;VARCHAR DEFAULT &apos;P_UPDATE_TABLE_STATISTICS&apos;;
&#x9;DECLARE sqlStatement&#x9;LONGVARCHAR;

&#x9;SET sqlStatement = &apos;update statistics &quot;&apos;||schemaName||&apos;&quot;.&quot;&apos;||tableName||&apos;&quot;&apos;;
&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;CALL print(moduleName||&apos;: sqlStatement=&apos;||NVL(sqlStatement,&apos;null&apos;));
&#x9;END IF;

&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;FOR r AS SELECT * FROM /shared/CoE/MonitorServer/Datasources/PackagedQueries/&quot;00_ExecuteDDL&quot;(sqlStatement) DO
&#x9;&#x9;&#x9;SET success = r.outputValue;
&#x9;&#x9;END FOR;
&#x9;EXCEPTION
&#x9;&#x9;ELSE RAISE;
&#x9;END;
END</attribute>
