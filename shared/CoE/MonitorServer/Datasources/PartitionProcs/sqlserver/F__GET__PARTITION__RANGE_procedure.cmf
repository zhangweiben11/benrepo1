PROCEDURE F_GET_PARTITION_RANGE(
&#x9;IN startDate &#x9;DATE,
&#x9;IN endDate &#x9;&#x9;DATE,
&#x9;-- NOTE: The partitionNum is the only colunm that is necessary.  The partitionDt only serves as a reference point for debugging.
&#x9;OUT results&#x9;&#x9;PIPE (partitionNum SMALLINT, partitionDt DATE)
)
BEGIN
&#x9;DECLARE databaseType&#x9;&#x9;VARCHAR DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.databaseType;
&#x9;DECLARE schemaName&#x9;&#x9;&#x9;VARCHAR DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.schemaName;
&#x9;DECLARE sqlStatement&#x9;&#x9;VARCHAR;
&#x9;DECLARE partMax &#x9;&#x9;&#x9;SMALLINT DEFAULT 366;
&#x9;DECLARE expectedCount &#x9;&#x9;INTEGER DEFAULT DATEDIFF(&apos;DAY&apos;, startDate, endDate);
&#x9;DECLARE cnt &#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT 0;
&#x9;DECLARE partition &#x9;&#x9;&#x9;SMALLINT;
&#x9;DECLARE currentDate &#x9;&#x9;DATE;
&#x9;DECLARE lastyear &#x9;&#x9;&#x9;SMALLINT;
&#x9;DECLARE compareDateMar01 &#x9;DATE;
&#x9;DECLARE isLeapYrLastYear &#x9;SMALLINT;
&#x9;DECLARE leapYearPartition &#x9;SMALLINT DEFAULT 60;
&#x9;DECLARE leapYearDate &#x9;&#x9;DATE;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;IF (expectedCount &lt; 0) THEN
&#x9;&#x9;SET expectedCount = partMax + expectedCount;
&#x9;END IF;

&#x9;-- Add 1 to the date so that the current date&apos;s partition is not returned and thus does not get truncated.
&#x9;SET currentDate = CAST(startDate AS DATE)+1;
&#x9;WHILE (cnt &lt;= expectedCount) DO
&#x9;&#x9;SET partition = null;
&#x9;&#x9;SET sqlStatement = &apos;SELECT [&apos;||schemaName||&apos;].[F_GET_PARTITION_NUM_COE] (&apos;&apos;&apos;||currentDate||&apos;&apos;&apos;)&apos;;
&#x9;&#x9;FOR r AS SELECT outputValue partitionNum FROM /shared/CoE/MonitorServer/Datasources/PackagedQueries/&quot;00_ExecuteDMLIntResult&quot;(sqlStatement) DO
&#x9;&#x9;&#x9;SET partition = r.partitionNum;
&#x9;&#x9;END FOR;
&#x9;&#x9;IF (partition IS NULL) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The variable &quot;partition&quot; cannot be null.&apos;;
&#x9;&#x9;END IF;

&#x9;&#x9;-- check if the previous year was leap year and clear out partition=60 when March 01 [0301] is selected in the list
&#x9;&#x9;SET compareDateMar01 = CAST(CAST(DATEPART(&apos;year&apos;, currentDate) AS VARCHAR(4))||&apos;0301&apos; AS DATE);
&#x9;&#x9;IF (currentDate = compareDateMar01) THEN
&#x9;&#x9;&#x9;SET lastyear = CAST(DATEPART(&apos;year&apos;, currentDate) AS SMALLINT) - 1;
&#x9;&#x9;&#x9;SET isLeapYrLastYear = 0;
&#x9;&#x9;&#x9;-- Check if last year was a leap year
&#x9;&#x9;&#x9;IF (EXTRACT(DAY FROM LAST_DAY(TO_DATE(lastyear||&apos;-02-01&apos;, &apos;yyyy-mm-dd&apos;))) = 29) THEN
&#x9;&#x9;&#x9;&#x9;SET isLeapYrLastYear = 1; 
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;-- If it was a leap year then insert a record to account for YYYY-02-29 and increment the counter[cnt]
&#x9;&#x9;&#x9;IF (isLeapYrLastYear = 1) THEN
&#x9;&#x9;&#x9;&#x9;SET leapYearDate = CAST((CAST(lastyear AS VARCHAR(4))||&apos;0229&apos;) AS DATE);
&#x9;&#x9;&#x9;&#x9;INSERT INTO results VALUES (leapYearPartition, leapYearDate) ;
&#x9;&#x9;&#x9;&#x9;SET cnt = cnt + 1;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;&#x9;INSERT INTO results VALUES (partition, currentDate);
&#x9;&#x9;SET currentDate = DATEADD(&apos;DAY&apos;,1,currentDate);
&#x9;&#x9;SET cnt = cnt + 1;
&#x9;END WHILE;
END </attribute>
