&#x9;csadmin-XXXX.jar, csarchive-XXXX.jar, csbase-XXXX.jar, csclient-XXXX.jar, cscommon-XXXX.jar,
&#x9;csext-XXXX.jar, csjdbc-XXXX.jar, csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar,
&#x9;and customproc-XXXX.jar (where -XXXX is an optional version number).  Any included third party files
&#x9;are licensed under the terms contained in their own accompanying LICENSE files, generally named .LICENSE.txt.
&#x9;
&#x9;This software is licensed AS-IS. Support for this software is not covered by standard maintenance agreements with TIBCO.
&#x9;If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
&#x9;agreement with TIBCO.
*/
PROCEDURE P_TRUNCATE_PARTITION( 
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1),
&#x9;IN schemaname&#x9;&#x9;&#x9;&#x9;VARCHAR,
&#x9;IN tablename&#x9;&#x9;&#x9;&#x9;VARCHAR,
&#x9;IN startDate&#x9;&#x9;&#x9;&#x9;TIMESTAMP,
&#x9;IN tableRetentionDays&#x9;&#x9;INTEGER,
&#x9;OUT rowcount&#x9;&#x9;&#x9;&#x9;INTEGER,
&#x9;OUT partitionList&#x9;&#x9;&#x9;LONGVARCHAR
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR DEFAULT &apos;P_TRUNCATE_PARTITION&apos;;
&#x9;DECLARE totalPartitionDays&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE endDate&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;TIMESTAMP;
&#x9;DECLARE msg &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE sqlStatement &#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE cnt &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT 0;

&#x9;-- Set the default number of partitions
&#x9;SET totalPartitionDays = 366;
&#x9;-- endDate - There are 366 partitions allowing 1 extra for leap year.  Calcuate the total partitions - number of days to keep history for.
&#x9;-- The startDate and endDate act as a rolling window into the 366 partitions
&#x9;SELECT DATEADD(&apos;day&apos;, totalPartitionDays - tableRetentionDays, startDate) INTO endDate from /services/databases/system/DUAL;

&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;SET msg = &apos;INPUT: schemaname=[&apos;||schemaname||&apos;]  tablename=[&apos;||tablename||&apos;]  start=[&apos;||CAST(startDate AS VARCHAR)||&apos;]  end=[&apos;||CAST(endDate AS VARCHAR)||&apos;]  totalPartitionDays=&apos;||totalPartitionDays||&apos;  (totalPartitionDays - purgeMetadata)=[&apos;||CAST(totalPartitionDays - tableRetentionDays AS VARCHAR)||&apos;]&apos;;
&#x9;&#x9;CALL PRINT (moduleName||&apos;: &apos;||msg);
&#x9;END IF;

&#x9;SET rowcount = 0;
&#x9;SET partitionList = &apos;&apos;;
&#x9;FOR r AS SELECT * FROM /shared/CoE/MonitorServer/Datasources/PartitionProcs/sqlserver/F_GET_PARTITION_RANGE(CAST(startDate AS DATE), CAST(endDate AS DATE)) DO
&#x9;&#x9;IF (LENGTH(partitionList) &gt; 0) THEN
&#x9;&#x9;&#x9;SET partitionList = partitionList || &apos;,&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;SET partitionList = partitionList || r.partitionNum;
&#x9;END FOR;

&#x9;-- Get the row count for the designated number of partitions that will be truncated
&#x9;IF (LENGTH(partitionList) &gt; 0) THEN
&#x9;&#x9;-- Get the partition row distribution
&#x9;&#x9;SET sqlStatement = 
&#x9;&#x9;&#x9;&#x9;&apos;        select p.partition_number &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;        from sys.tables t &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;            join sys.indexes i on t.object_id = i.object_id &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;            join sys.partitions p on i.object_id = p.object_id and i.index_id = p.index_id &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;        where i.index_id &lt; 2  &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;          and i.object_id = object_id(&apos;&apos;&quot;&apos;||schemaname||&apos;&quot;.&quot;&apos;||tablename||&apos;&quot;&apos;&apos;) &apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;          and p.rows &gt; 0&apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;          and p.partition_number IN (&apos;||partitionList||&apos;)&apos;||CHR(10)||
&#x9;&#x9;&#x9;&#x9;&apos;        order by p.partition_number&apos;;

&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;SET msg = &apos;GET PARTITION LIST: sqlStatement=&apos;||CHR(10)||NVL(sqlStatement,&apos;null&apos;);
&#x9;&#x9;&#x9;CALL PRINT (moduleName||&apos;: &apos;||msg);
&#x9;&#x9;END IF;

&#x9;&#x9;-- Create a list of partitions that have data (non-zero)
&#x9;&#x9;SET partitionList = &apos;&apos;;
&#x9;&#x9;SET rowcount = 0;
&#x9;&#x9;FOR r AS SELECT outputValue partitionNum FROM /shared/CoE/MonitorServer/Datasources/PackagedQueries/&quot;00_ExecuteDMLIntResult&quot;(sqlStatement) DO
&#x9;&#x9;&#x9;SET rowcount = rowcount + 1;
&#x9;&#x9;&#x9;IF (LENGTH(partitionList) &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;SET partitionList = partitionList || &apos;,&apos;;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;SET partitionList = partitionList || r.partitionNum;
&#x9;&#x9;END FOR;

&#x9;&#x9;-- Only execute if there are rows to truncate
&#x9;&#x9;IF rowcount &gt; 0 THEN
&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET msg = &apos;TRUNCATE PARTITIONS: rowcount=&apos;||rowcount||&apos;  partitionList=[&apos;||NVL(partitionList,&apos;null&apos;)||&apos;]&apos;;
&#x9;&#x9;&#x9;&#x9;CALL PRINT (moduleName||&apos;: &apos;||msg);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;-- Truncate the designated partitioins
&#x9;&#x9;&#x9;FOR r AS 
&#x9;&#x9;&#x9;&#x9;SELECT textExpression partitionNum FROM /shared/ASAssets/Utilities/string/extractTextList(partitionList, &apos;,&apos;, 0,0,0,0,1)
&#x9;&#x9;&#x9;&#x9;WHERE textExpression IS NOT NULL AND LENGTH(TRIM(textExpression)) &gt; 0
&#x9;&#x9;&#x9;DO
&#x9;&#x9;&#x9;&#x9;-- Construct the TRUNCATE TABLE dynamic sql statement
&#x9;&#x9;&#x9;&#x9;SET sqlStatement = &apos;TRUNCATE TABLE &quot;&apos;||schemaname||&apos;&quot;.&quot;&apos;||tablename||&apos;&quot; WITH (PARTITIONS(&apos;||r.partitionNum||&apos;))&apos;;
&#x9;&#x9;&#x9;&#x9;FOR c AS SELECT * FROM /shared/CoE/MonitorServer/Datasources/PackagedQueries/&quot;00_ExecuteDDL&quot;(sqlStatement) DO
&#x9;&#x9;&#x9;&#x9;END FOR;
&#x9;&#x9;&#x9;END FOR;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET msg = &apos;NO PARTITIONS TO TRUNCATE&apos;;
&#x9;&#x9;&#x9;&#x9;CALL PRINT (moduleName||&apos;: &apos;||msg);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;-- Set partitionList to null if blank
&#x9;IF (LENGTH(partitionList) = 0) THEN
&#x9;&#x9;SET partitionList = null;
&#x9;END IF;
&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;CALL print(&apos;&apos;);
&#x9;END IF;
END </attribute>
