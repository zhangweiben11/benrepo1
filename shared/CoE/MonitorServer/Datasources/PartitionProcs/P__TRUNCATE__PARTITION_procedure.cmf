PROCEDURE P_TRUNCATE_PARTITION(
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1),  &#x9;&#x9;&#x9;-- Debug: Y=debug on.  N=debug off
&#x9;OUT truncateCount&#x9;&#x9;&#x9;&#x9;INTEGER,
&#x9;OUT truncatePartitionList&#x9;&#x9;LONGVARCHAR
)
BEGIN
&#x9;DECLARE schemaName&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR&#x9;&#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.schemaName;
&#x9;DECLARE databaseType&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR&#x9;&#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.databaseType;
&#x9;DECLARE tableRetentionDays&#x9;&#x9;&#x9;&#x9;INTEGER&#x9;&#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.tableRetentionDays;
&#x9;DECLARE HealthCheckTable &#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.HealthCheckTable;
&#x9;DECLARE SystemRequestsTable&#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.SystemRequestsTable;
&#x9;DECLARE SystemEventTable&#x9;&#x9;&#x9;&#x9;VARCHAR &#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.SystemEventTable;
&#x9;DECLARE tableList&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR&#x9;DEFAULT HealthCheckTable||&apos;,&apos;||SystemRequestsTable||&apos;,&apos;||SystemEventTable;
&#x9;DECLARE tableName&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE currentTime&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;TIMESTAMP;
&#x9;DECLARE errorMessage&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;

&#x9;-- Get the current time
&#x9;CALL /shared/ASAssets/Utilities/&quot;time&quot;/getCurrentTimestamp(currentTime);

&#x9;-- Extract each table from the partition list
&#x9;FOR r AS SELECT object tableName FROM /shared/ASAssets/Utilities/string/extractDelimitedText(tableList, &apos;,&apos;, 0,0,0,1,0) DO
&#x9;&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;&#x9;SET tableName = r.tableName;
&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;  TRUNCATE TABLE=[&apos;||tableName||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;&apos;);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;IF (LOWER(databaseType) = &apos;oracle&apos;) THEN
&#x9;&#x9;&#x9;&#x9;-- Truncate partitions
&#x9;&#x9;&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/oracle/P_TRUNCATE_PARTITION(debug, schemaName, tableName, currentTime, tableRetentionDays, truncateCount, truncatePartitionList);
&#x9;&#x9;&#x9;ELSEIF (LOWER(databaseType) = &apos;sqlserver&apos;) THEN
&#x9;&#x9;&#x9;&#x9;-- Truncate partitions
&#x9;&#x9;&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/sqlserver/P_TRUNCATE_PARTITION(debug, schemaName, tableName, currentTime, tableRetentionDays, truncateCount, truncatePartitionList);
&#x9;&#x9;&#x9;ELSEIF (LOWER(databaseType) = &apos;postgres&apos;) THEN
&#x9;&#x9;&#x9;&#x9;-- Truncate partitions
&#x9;&#x9;&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/postgres/P_TRUNCATE_PARTITION(debug, schemaName, tableName, currentTime, tableRetentionDays, truncateCount, truncatePartitionList);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;IF (truncateCount &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;-- Generate a system user-defined event SUCCESS
&#x9;&#x9;&#x9;&#x9;CALL /lib/util/GenerateEvent(&apos;TruncatePartitionSuccess&apos;,&apos;SUCCESS: TRUNCATE PARTIONS TABLE=&apos;||NVL(tableName,&apos;null&apos;)||&apos;  COUNT=&apos;||truncateCount||&apos;  PARTITION LIST=&apos;||NVL(truncatePartitionList,&apos;null&apos;));
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;  truncateCount=&apos;||truncateCount||&apos;  truncatePartitionList=&apos;||NVL(truncatePartitionList,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;&apos;);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;EXCEPTION
&#x9;&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;&#x9;SET errorMessage = CAST(CURRENT_EXCEPTION.TRACE AS LONGVARCHAR);
&#x9;&#x9;&#x9;&#x9;-- Generate a system user-defined event SUCCESS
&#x9;&#x9;&#x9;&#x9;CALL /lib/util/GenerateEvent(&apos;TruncatePartitionFailure&apos;,&apos;FAILURE: TABLE=&apos;||NVL(tableName,&apos;null&apos;)||&apos;  EXCEPTION=[&apos;||NVL(REPLACE(errorMessage, CHR(10), &apos; &apos;),&apos;null&apos;)||&apos;]&apos;);&#x9;
&#x9;&#x9;END;
&#x9;END FOR;
END</attribute>
