PROCEDURE P_UPDATE_STATISTICS(
&#x9;IN debug&#x9;&#x9;&#x9;CHAR(1),
&#x9;IN schemaName&#x9;&#x9;VARCHAR,
&#x9;IN tableName&#x9;&#x9;VARCHAR,
&#x9;OUT success&#x9;&#x9;&#x9;INTEGER
)
BEGIN
&#x9;DECLARE databaseType&#x9;&#x9;VARCHAR&#x9;DEFAULT /shared/CoE/MonitorServer/Customize/commonValues.databaseType;

&#x9;IF (LOWER(databaseType) = &apos;oracle&apos;) THEN
&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/oracle/P_UPDATE_TABLE_STATISTICS(debug, schemaName, tableName, success);
&#x9;ELSEIF (LOWER(databaseType) = &apos;sqlserver&apos;) THEN
&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/sqlserver/P_UPDATE_TABLE_STATISTICS(debug, schemaName, tableName, success);
&#x9;ELSEIF (LOWER(databaseType) = &apos;postgres&apos;) THEN
&#x9;&#x9;CALL /shared/CoE/MonitorServer/Datasources/PartitionProcs/postgres/P_UPDATE_TABLE_STATISTICS(debug, schemaName, tableName, success);
&#x9;END IF;

END</attribute>
