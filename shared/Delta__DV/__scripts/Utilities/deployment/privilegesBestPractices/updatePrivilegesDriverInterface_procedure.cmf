&#x9;&#x9;&#x9;SET pVect[i].Name_ = null;
&#x9;&#x9;&#x9;IF ((pos2-pos1) &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;SET pVect[i].Name_ = TRIM(SUBSTRING(inputStr, pos1, pos2-pos1));
