&#x9;&#x9;END IF;

&#x9;&#x9;-- Concatenate the logOutput
&#x9;&#x9;SET logOutputResult = logOutputResult || &apos;executeNum=&apos;||i || &apos;|&apos; || 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &apos;rowStatus=&apos;||rowStatus || &apos;|&apos; || 
