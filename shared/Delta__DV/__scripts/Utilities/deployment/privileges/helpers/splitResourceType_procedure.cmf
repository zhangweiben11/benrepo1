PROCEDURE splitResourceType(
&#x9;IN ownerPathList LONGVARCHAR, 
&#x9;OUT outPipe &#x9;PIPE (
&#x9;&#x9;ownerName &#x9;VARCHAR, 
&#x9;&#x9;ownerDomain VARCHAR, 
&#x9;&#x9;rPath &#x9;&#x9;LONGVARCHAR, 
&#x9;&#x9;rType &#x9;&#x9;LONGVARCHAR
&#x9;)
)
BEGIN
&#x9;DECLARE ownerName&#x9;VARCHAR;
&#x9;DECLARE ownerDomain&#x9;VARCHAR;
&#x9;DECLARE resPath&#x9;&#x9;VARCHAR(4000);
&#x9;DECLARE resType&#x9;&#x9;VARCHAR;

&#x9;FOR r AS
&#x9;&#x9;SELECT * FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractTextList(ownerPathList, &apos;,&apos;, 1, 0, 0, 0, 1)
&#x9;&#x9;WHERE textExpression IS NOT NULL AND LENGTH(TRIM(textExpression)) &gt; 0
&#x9;DO
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/getDelimitedOccurrence(r.textExpression, &apos;F&apos;, &apos;:&apos;, 1, 1, ownerName);
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/getDelimitedOccurrence(r.textExpression, &apos;F&apos;, &apos;:&apos;, 2, 1, ownerDomain);
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/getDelimitedOccurrence(r.textExpression, &apos;F&apos;, &apos;:&apos;, 3, 1, resPath);
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/getDelimitedOccurrence(r.textExpression, &apos;F&apos;, &apos;:&apos;, 4, 1, resType);
&#x9;&#x9;-- Remove any surrounding double quotes
&#x9;&#x9;IF (INSTR(resPath, &apos;&quot;&apos;) = 1 AND INSTR(REVERSE(resPath), &apos;&quot;&apos;) = 1) THEN
&#x9;&#x9;&#x9;SET resPath = SUBSTRING(resPath, 2, LENGTH(resPath)-2);
&#x9;&#x9;END IF;
 &#x9;&#x9;-- Insert into the pipe
&#x9;&#x9;INSERT INTO outPipe VALUES (ownerName, ownerDomain, resPath, resType);
&#x9;END FOR;
END</attribute>
