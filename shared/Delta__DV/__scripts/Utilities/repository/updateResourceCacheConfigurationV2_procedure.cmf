&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;CALL PRINT(moduleName||&apos; : is621=&apos;||NVL(CAST(is621 AS VARCHAR),&apos;null&apos;));
&#x9;&#x9;CALL PRINT(moduleName||&apos; : inResourcePath=&apos;||NVL(inResourcePath,&apos;null&apos;));
