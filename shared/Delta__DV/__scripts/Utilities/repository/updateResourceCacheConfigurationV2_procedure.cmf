&#x9;&#x9;CALL PRINT(moduleName||&apos; : inConfigured=&apos;||NVL(CAST(inConfigured AS VARCHAR),&apos;null&apos;));
&#x9;&#x9;CALL PRINT(moduleName||&apos; : inEnabled=&apos;||NVL(CAST(inEnabled AS VARCHAR),&apos;null&apos;));
&#x9;&#x9;CALL PRINT(moduleName||&apos; : inStorageMode=&apos;||NVL(inStorageMode,&apos;null&apos;));
&#x9;&#x9;CALL PRINT(moduleName||&apos; : inStorageDataSourcePath=&apos;||NVL(inStorageDataSourcePath,&apos;null&apos;));
&#x9;&#x9;CALL PRINT(moduleName||&apos; : storageTargets=&apos;||NVL(storageTargets,&apos;null&apos;));
