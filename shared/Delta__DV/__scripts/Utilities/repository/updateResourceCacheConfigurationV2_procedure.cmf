&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/getCisVersion(cisVersion);
&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/TextUtils/RegexPosition(cisVersion, &apos;^(?:6\.2\.[1-9]|[7-9]|[1-9][0-9])&apos;, 1, pos1);
&#x9;IF (pos1 = 1) THEN
