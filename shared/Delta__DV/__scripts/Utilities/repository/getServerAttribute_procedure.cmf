PROCEDURE getServerAttribute(
    IN attribute &#x9;VARCHAR(4096),
    OUT result &#x9;&#x9;PIPE (
        attrName VARCHAR(32768), 
        attrType VARCHAR(32768), 
        attrValue VARCHAR(32768), 
        listEntryType VARCHAR(32768), 
        listEntryValue VARCHAR(32768), 
        mapKeyType VARCHAR(32768), 
        mapKeyValue VARCHAR(32768), 
        mapValueType VARCHAR(32768), 
        mapValueValue VARCHAR(32768), 
        arrayItem VARCHAR(32768)
&#x9;)
)
BEGIN
&#x9;DECLARE numRows&#x9;&#x9;INTEGER DEFAULT 0;
    DECLARE PUBLIC TYPE ResultRowType ROW (
        attrName VARCHAR(32768), 
        attrType VARCHAR(32768), 
        attrValue VARCHAR(32768), 
        listEntryType VARCHAR(32768), 
        listEntryValue VARCHAR(32768), 
        mapKeyType VARCHAR(32768), 
        mapKeyValue VARCHAR(32768), 
        mapValueType VARCHAR(32768), 
        mapValueValue VARCHAR(32768), 
        arrayItem VARCHAR(32768)
    );
&#x9;DECLARE currRec&#x9;&#x9;ResultRowType;
&#x9;DECLARE prevRec&#x9;&#x9;ResultRowType;

    FOR r AS
        SELECT * FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/lowerLevelProcedures/getServerAttributesXSLT(
&apos;&lt;server:getServerAttributes xmlns:server=&quot;http://www.compositesw.com/services/system/admin/server&quot;&gt;
  &lt;server:paths&gt;
    &lt;server:path&gt;&apos; || CAST(XMLTEXT(COALESCE(TRIM(attribute), &apos;&apos;)) AS VARCHAR(4096)) || &apos;&lt;/server:path&gt;
  &lt;/server:paths&gt;
&lt;/server:getServerAttributes&gt;&apos;
        )
&#x9;DO
&#x9;&#x9;SET currRec = r;
&#x9;&#x9;SET numRows = numRows + 1;
&#x9;&#x9;IF (r.attrType IN (&apos;LIST&apos;,&apos;MAP&apos;,&apos;STRING_ARRAY&apos;)) THEN
&#x9;&#x9;&#x9;IF (numRows &gt; 1) THEN
&#x9;&#x9;&#x9;&#x9;INSERT INTO result VALUES(prevRec);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;INSERT INTO result VALUES(currRec);
&#x9;&#x9;END IF;
&#x9;&#x9;SET prevRec = currRec;
&#x9;END FOR;

&#x9;IF ((prevRec.attrType IN (&apos;LIST&apos;,&apos;MAP&apos;,&apos;STRING_ARRAY&apos;) AND numRows &gt; 1 AND
&#x9;&#x9;(prevRec.attrValue IS NOT NULL AND prevRec.listEntryValue IS NOT NULL AND prevRec.mapKeyValue IS NOT NULL AND prevRec.mapValueValue IS NOT NULL AND prevRec.arrayItem IS NOT NULL)) 
&#x9;&#x9;OR
&#x9;&#x9;(prevRec.attrType IN (&apos;LIST&apos;,&apos;MAP&apos;,&apos;STRING_ARRAY&apos;) AND numRows = 1 AND
&#x9;&#x9;(prevRec.attrValue IS NULL AND prevRec.listEntryValue IS NULL AND prevRec.mapKeyValue IS NULL AND prevRec.mapValueValue IS NULL AND prevRec.arrayItem IS NULL))
&#x9;) THEN
&#x9;&#x9;INSERT INTO result VALUES(prevRec);
&#x9;END IF;
END</attribute>
