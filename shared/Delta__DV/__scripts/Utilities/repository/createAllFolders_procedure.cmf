PROCEDURE createAllFolders(
&#x9;-- /shared/AbstractViewGenerator/AbstractLayer/Inventory
&#x9;IN inSourceFolderPath &#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType,
&#x9;OUT success&#x9;&#x9;BIT,
&#x9;OUT faultResponse&#x9;XML
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;VARCHAR DEFAULT &apos;createAllFolders&apos;;
&#x9;DECLARE debug&#x9;&#x9;&#x9;&#x9;CHAR(1) DEFAULT /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug;
&#x9;DECLARE sourceFolderPath&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType DEFAULT TRIM(inSourceFolderPath);
&#x9;DECLARE folderName &#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE prevFolderName &#x9;&#x9;VARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE folderPath &#x9;&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE folderSubtype&#x9;&#x9;VARCHAR;
&#x9;DECLARE parentPath&#x9;&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE dataServicePath&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE catalogPath&#x9;&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE schemaPath&#x9;&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE i &#x9;&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE createInput &#x9;&#x9;VARCHAR(8128);
&#x9;DECLARE createResponse &#x9;&#x9;XML;
&#x9;DECLARE resourceExists&#x9;&#x9;BOOLEAN;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;set success = 1;
&#x9;if (sourceFolderPath IS NULL or LENGTH(sourceFolderPath) = 0) then
&#x9;&#x9;raise ex value &apos;Source input path may not be null or empty.&apos;;
&#x9;end if;
&#x9;set sourceFolderPath = REPLACE(sourceFolderPath,&apos;\&apos;,&apos;/&apos;);  -- Normalize separators to make it easier
&#x9;-- A path should look like this: /shared/myfolder/subfolder/lastfolder
&#x9;-- Add leading / but only if a windows drive is not present such as c:
&#x9;if (SUBSTRING(sourceFolderPath,1,1) &lt;&gt; &apos;/&apos; and POSITION(&apos;:&apos; in sourceFolderPath) = 0) then
&#x9;&#x9;set sourceFolderPath = &apos;/&apos;||sourceFolderPath;
    end if;
    -- Remove trailing /
&#x9;if (SUBSTRING(sourceFolderPath,length(sourceFolderPath),1) = &apos;/&apos;) then
&#x9;&#x9;set sourceFolderPath = SUBSTRING(sourceFolderPath,1,length(sourceFolderPath)-1);  
&#x9;end if;
&#x9;-- remove double quotes from the path
&#x9;if (POSITION(&apos;&quot;&apos; IN sourceFolderPath) &gt; 0) THEN
    &#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug, &apos;Removing double quote(s) from the path.&apos;);
&#x9;&#x9;set sourceFolderPath = REPLACE(sourceFolderPath, &apos;&quot;&apos;, &apos;&apos;);
&#x9;end if;

&#x9;-- If this is a published datasource path then determine how many folders are present which can be used to determine 
&#x9;--&#x9;&#x9;if the following containers are catalogs or schemas.
&#x9;-- There are two possibilities with published databases.  1) Catalog + Schema, 2) Schema only
&#x9;-- &#x9;&#x9;1) /services/databases/PUBDB/CATALOG/SCHEMA
&#x9;-- &#x9;&#x9;2) /services/databases/PUBDB/SCHEMA
&#x9;if (INSTR(sourceFolderPath, &apos;/services/databases/&apos;) &gt; 0) then
&#x9;&#x9;set folderPath = &apos;&apos;;
&#x9;&#x9;set i = 0;
&#x9;&#x9;for r as 
&#x9;&#x9;&#x9;select object folderPart
&#x9;&#x9;&#x9;&#x9;from /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(sourceFolderPath,&apos;/&apos;,0,0,0,1,0)
&#x9;&#x9;&#x9;&#x9;where object is not null and length(object) &gt; 0
&#x9;&#x9;do
&#x9;&#x9;&#x9;set folderPath = folderPath || &apos;/&apos; || r.folderPart;
&#x9;&#x9;&#x9;set i = i + 1;
&#x9;&#x9;&#x9;if (i = 3) then
&#x9;&#x9;&#x9;&#x9;set dataServicePath = folderPath;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;if (i = 4) then
&#x9;&#x9;&#x9;&#x9;set catalogPath = folderPath;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;if (i = 5) then
&#x9;&#x9;&#x9;&#x9;set schemaPath = folderPath;
&#x9;&#x9;&#x9;end if;&#x9;&#x9;
&#x9;&#x9;end for;
&#x9;&#x9;if (i = 4) then
&#x9;&#x9;&#x9;set schemaPath = catalogPath;
&#x9;&#x9;&#x9;set catalogPath = null;
&#x9;&#x9;end if;
&#x9;&#x9;if (debug = &apos;Y&apos;) then
&#x9;&#x9;&#x9;call print(moduleName||&apos; : dataServicePath=&apos;||TRIM(NVL(dataServicePath,&apos; &apos;)));
&#x9;&#x9;&#x9;call print(moduleName||&apos; : catalogPath=&apos;||TRIM(NVL(catalogPath,&apos; &apos;)));
&#x9;&#x9;&#x9;call print(moduleName||&apos; : schemaPath=&apos;||TRIM(NVL(schemaPath,&apos; &apos;)));
&#x9;&#x9;end if;
&#x9;end if;

&#x9;-- Process the path
&#x9;set folderPath = &apos;&apos;;
&#x9;set parentPath = &apos;&apos;;
&#x9;set i = 0;
&#x9;for r as 
&#x9;&#x9;select object folderPart
&#x9;&#x9;&#x9;from /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(sourceFolderPath,&apos;/&apos;,0,0,0,1,0)
&#x9;&#x9;&#x9;where object is not null and length(object) &gt; 0
&#x9;do
&#x9;&#x9;set i = i + 1;
&#x9;&#x9;-- Set the folder path and folder name and parent path
&#x9;&#x9;set folderName = r.folderPart;
&#x9;&#x9;set folderPath = folderPath || &apos;/&apos; || folderName;
&#x9;&#x9;if (i &gt; 1) then
&#x9;&#x9;&#x9;set parentPath = parentPath || &apos;/&apos; || prevFolderName;
&#x9;&#x9;end if;
&#x9;&#x9;if (debug = &apos;Y&apos;) then
&#x9;&#x9;&#x9;call print(moduleName||&apos; : folderName=&apos;||folderName);
&#x9;&#x9;&#x9;call print(moduleName||&apos; : folderPath=&apos;||folderPath);
&#x9;&#x9;&#x9;call print(moduleName||&apos; : parentPath=&apos;||parentPath);
&#x9;&#x9;end if;

&#x9;&#x9;-- Don&apos;t bother trying to create system folders
&#x9;&#x9;if (folderPath &lt;&gt; &apos;/shared&apos; and folderPath &lt;&gt; &apos;/services&apos; and 
&#x9;&#x9;&#x9;folderPath &lt;&gt; &apos;/services/databases&apos; and folderPath &lt;&gt; &apos;/services/webservices&apos; and 
&#x9;&#x9;&#x9;folderPath &lt;&gt; &apos;/users&apos; and folderPath &lt;&gt; &apos;/lib&apos; and folderPath &lt;&gt; &apos;/policy&apos;) then
&#x9;&#x9;&#x9;-- Check for a DATA_SOURCE type parent path
&#x9;&#x9;&#x9;if (parentPath = &apos;/services/databases&apos;) then
 &#x9;&#x9;&#x9;&#x9;call /lib/resource/ResourceExists(folderPath, &apos;DATA_SOURCE&apos;, null, resourceExists);
&#x9;&#x9;&#x9;&#x9;if (NOT resourceExists) then
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug,&apos;Creating published datasource:  folderPath=&apos;||folderPath);
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/createDataSource(parentPath, folderName, &apos;COMPOSITE_DATABASE&apos;, success, createResponse, faultResponse);
&#x9;&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug,&apos;Published datasource already exists:  folderPath=&apos;||folderPath);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;set folderSubtype = &apos;FOLDER_CONTAINER&apos;;
&#x9;&#x9;&#x9;&#x9;if (INSTR(parentPath, &apos;/services/databases/&apos;) &gt; 0) then
&#x9;&#x9;&#x9;&#x9;&#x9;if (folderPath = schemaPath) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set folderSubtype = &apos;SCHEMA_CONTAINER&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;if (folderPath = catalogPath) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set folderSubtype = &apos;CATALOG_CONTAINER&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;end if;

 &#x9;&#x9;&#x9;&#x9;call /lib/resource/ResourceExists(folderPath, &apos;CONTAINER&apos;, null, resourceExists);
&#x9;&#x9;&#x9;&#x9;if (NOT resourceExists) then
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug,&apos;Creating folder:  folderPath=&apos;||folderPath);
&#x9;&#x9;&#x9;&#x9;&#x9;set createInput = 
                        &apos;&lt;resource:createResource xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot;&gt;&apos;||CHR(10)||
                        &apos;    &lt;resource:path&gt;&apos; || CAST(XMLTEXT(parentPath) AS LONGVARCHAR) || &apos;&lt;/resource:path&gt;&apos;||CHR(10)||
                        &apos;    &lt;resource:name&gt;&apos; || CAST(XMLTEXT(folderName) AS LONGVARCHAR) || &apos;&lt;/resource:name&gt;&apos;||CHR(10)||
                        &apos;    &lt;resource:detail&gt;FULL&lt;/resource:detail&gt;&apos;||CHR(10)||
                        &apos;    &lt;resource:type&gt;CONTAINER&lt;/resource:type&gt;&apos;||CHR(10)||
                        &apos;    &lt;resource:subtype&gt;&apos;||folderSubtype||&apos;&lt;/resource:subtype&gt;&apos;||CHR(10)||
                        &apos;&lt;/resource:createResource&gt;&apos;;

&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug, createInput);
&#x9;&#x9;&#x9;&#x9;&#x9;call /services/webservices/system/admin/resource/operations/createResource(createInput, createResponse, faultResponse);
&#x9;&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug,&apos;Folder already exists:  folderPath=&apos;||folderPath);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (faultResponse is not null) then
&#x9;&#x9;&#x9;&#x9;&#x9;set success = 0;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;end if;
&#x9;&#x9;set prevFolderName = folderName;
&#x9;end for;
END</attribute>
