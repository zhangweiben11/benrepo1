        &lt;displayName&gt;Maximum Keys to Check Between Two Tables for Multicolumn Key&lt;/displayName&gt;
        &lt;maxValue&gt;1000000&lt;/maxValue&gt;
        &lt;minValue&gt;10&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
