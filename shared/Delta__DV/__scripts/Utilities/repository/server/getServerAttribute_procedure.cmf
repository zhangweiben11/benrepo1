        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Allow Multicolumn Key&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;discovery10&lt;/id&gt;
        &lt;name&gt;/discovery/relationship/multicolumnkey/checkOnlyManyToMany&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
