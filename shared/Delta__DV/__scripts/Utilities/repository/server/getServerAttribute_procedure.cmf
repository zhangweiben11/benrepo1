        &lt;displayName&gt;Check Only Many-to-Many Relationships&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;discovery11&lt;/id&gt;
