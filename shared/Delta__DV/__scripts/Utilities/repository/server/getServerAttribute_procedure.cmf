    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server37&lt;/id&gt;
        &lt;name&gt;/server/config/cache/failoverToOriginalDatasource&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Applicable to cached resources using a relational cache. When set to true, if the cache is not accessible, the resources will be treated as not being cached.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Failover To Original Data Sources&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server38&lt;/id&gt;
        &lt;name&gt;/server/config/cache/garbageCollectionDelaySeconds&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Collect and then delete the cached data after this many seconds. Too aggressively delete data could lead to cache refresh errors in cluster mode.
    &lt;/annotation&gt;
        &lt;defaultValue&gt;0&lt;/defaultValue&gt;
        &lt;displayName&gt;Garbage Collection Delay Seconds&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server39&lt;/id&gt;
        &lt;name&gt;/server/config/cache/loading/enableNativeLoading&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Indicates whether cache loading using native mechanisms, such as Oracle Database Links, is enabled.&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Native Loading&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server40&lt;/id&gt;
        &lt;name&gt;/server/config/cache/loading/enableParallelLoading&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Indicates whether parallel cache loading is enabled.&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Parallel Loading&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server41&lt;/id&gt;
        &lt;name&gt;/server/config/cache/statusSyncMaxInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Maximum allowed interval between syncing in memory cache status with that in database table. This need not be set for automatic caching.
    &lt;/annotation&gt;
        &lt;defaultValue&gt;3600&lt;/defaultValue&gt;
        &lt;displayName&gt;Cache status sync interval seconds&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server42&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/checkConnectionInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Time threshold in minutes beyond which the Server considers itself BLOCKED if it has not received a pending response.&lt;/annotation&gt;
        &lt;defaultValue&gt;60&lt;/defaultValue&gt;
        &lt;displayName&gt;Block Threshold&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;minutes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server43&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/displayName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A user-editable name of the cluster.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Cluster Display Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server44&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/globalCid&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The current global change id.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Global Change Id&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server45&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/healthmonitor/aggressiveHeartbeatInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Once any node is flagged as down how often to check for heartbeat of other nodes, in seconds&lt;/annotation&gt;
        &lt;defaultValue&gt;300&lt;/defaultValue&gt;
        &lt;displayName&gt;Health Monitor Aggressive Heartbeat Interval&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server46&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/healthmonitor/db/heartbeatTable&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Health Monitor heart beat table path in CIS&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Health Monitor Heartbeat Table Path in CIS&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server47&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/healthmonitor/heartbeatInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Number of seconds between heartbeats&lt;/annotation&gt;
        &lt;defaultValue&gt;600&lt;/defaultValue&gt;
        &lt;displayName&gt;Health Monitor Heartbeat Interval&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server48&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/healthmonitor/heartbeatReadInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Number of seconds to wait after sending a heartbeat before reading heartbeats of other nodes in cluster&lt;/annotation&gt;
        &lt;defaultValue&gt;30&lt;/defaultValue&gt;
        &lt;displayName&gt;Health Monitor Heartbeat Read Interval&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server49&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/healthmonitor/tolerance&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Number of seconds to wait once a node is found to be unresponsive before flagging it as down&lt;/annotation&gt;
        &lt;defaultValue&gt;600&lt;/defaultValue&gt;
        &lt;displayName&gt;Health Monitor Tolerance Interval&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server50&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/heartbeatInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Number of seconds between heartbeat messages Server sends out to other cluster members.&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Cluster Heartbeat Interval&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server51&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/heartbeatIntervalOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Number of seconds between heartbeat messages Server sends out to other cluster members.&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Cluster Heartbeat Interval (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server52&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/loggingLevel&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Can be set to OFF (no detailed logging is enabled), MEMBER (log member and timekeeper changes only), METADATA (in addition to member and timekeeper changes, log metadata changes as well), ALL (log all cluster communications) &lt;/annotation&gt;
        &lt;defaultValue&gt;OFF&lt;/defaultValue&gt;
        &lt;displayName&gt;Cluster Logging Detail Level &lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server53&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/name&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;An internally-generated name to uniquely identify a cluster, constructed as &quot;creationtime-hashCodeOfServerId&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Cluster System Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server54&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/retentionPeriod&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The minimum number of hours the Server will keep a metadata object after it is deleted. This is needed only if some server(s) in the cluster are not up all the time, or the network among the cluster members are intermittent.  This setting is ignored if all servers in the cluster are running and communicating with each other.&lt;/annotation&gt;
        &lt;defaultValue&gt;72&lt;/defaultValue&gt;
        &lt;displayName&gt;Repository Retention Period&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;hours&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server55&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/retryInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The time interval in seconds between the Server&apos;s attempts to contact a non-responding remote cluster node.&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Connection Retry Interval&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server56&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/serverId&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;A 32-bit value to uniquely identify a server node in a cluster.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;-1&lt;/defaultValue&gt;
        &lt;displayName&gt;Server ID&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server57&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/timeout&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The number of seconds the Server will wait for an initial response while establishing a connection to a remote cluster node.&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Initial Response Timeout&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server58&lt;/id&gt;
        &lt;name&gt;/server/config/cluster/triggerDistribution/timeKeeperWeight&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Set this if time keeper needs special weight in weighted round robin trigger distribution. For most use cases this value need not be set.&lt;/annotation&gt;
        &lt;defaultValue&gt;1&lt;/defaultValue&gt;
        &lt;displayName&gt;Weight of time keeper&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server59&lt;/id&gt;
        &lt;name&gt;/server/config/database/bundled&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Indicates whether or not the repository database is the one bundled with the product.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Bundled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server60&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/connectionValidationQuery&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Connection Validation Query would run on repository database and if successful would validate the connection to the repository database.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;SELECT 1 FROM metadata_version&lt;/defaultValue&gt;
        &lt;displayName&gt;Connection Validation Query&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server61&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolInitialSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Initial number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be between the &quot;Minimum&quot; and &quot;Maximum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Initial Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server62&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolInitialSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Initial number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be between the &quot;Minimum&quot; and &quot;Maximum&quot;.  If this value is less than the &quot;Minimum&quot; or greater than &quot;Maximum&quot;, it will be reset to the value of &quot;Minimum&quot; to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Initial Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server63&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolMaxSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Maximum number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be greater than the &quot;Minimum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Maximum Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server64&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolMaxSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Maximum number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be greater than the &quot;Minimum&quot;.  If this value is less than the &quot;Minimum&quot;, the value of &quot;Minimum&quot; will be reset to this value to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Maximum Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server65&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolMinSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Minimum number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be less than the &quot;Maximum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Minimum Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server66&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemPool/poolMinSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Minimum number of internal database connections opened for use by the Server metadata sub-system. Note that this number must be less than the &quot;Maximum&quot;.  If this value is greater than the &quot;Maximum&quot;, it will be reset to the value of &quot;Maximum&quot; to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Minimum Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server67&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/connectionValidationQuery&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Connection Validation Query would run on repository database and if successful would validate the connection to the repository database.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;SELECT 1 FROM metadata_version&lt;/defaultValue&gt;
        &lt;displayName&gt;Connection Validation Query&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server68&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolInitialSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Initial number of internal database connections opened for client access to system tables. Note that this number must be between the &quot;Minimum&quot; and &quot;Maximum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Initial Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server69&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolInitialSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Initial number of internal database connections opened for client access to system tables. Note that this number must be between the &quot;Minimum&quot; and &quot;Maximum&quot;.  If this value is less than the &quot;Minimum&quot; or greater than &quot;Maximum&quot;, it will be reset to the value of &quot;Minimum&quot; to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Initial Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server70&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolMaxSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Maximum number of internal database connections opened for client access to system tables. Note that this number must be greater than the &quot;Minimum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Maximum Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server71&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolMaxSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Maximum number of internal database connections opened for client access to system tables. Note that this number must be greater than the &quot;Minimum&quot;.  If this value is less than the &quot;Minimum&quot;, the value of &quot;Minimum&quot; will be reset to this value to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Maximum Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server72&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolMinSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Minimum number of internal database connections opened for client access to system tables. Note that this number must be less than the &quot;Maximum&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Minimum Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server73&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionPools/systemTablePool/poolMinSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Minimum number of internal database connections opened for client access to system tables. Note that this number must be less than the &quot;Maximum&quot;.  If this value is greater than the &quot;Maximum&quot;, it will be reset to the value of &quot;Maximum&quot; to ensure that the server can be started successfully.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;5&lt;/defaultValue&gt;
        &lt;displayName&gt;Pool Minimum Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server74&lt;/id&gt;
        &lt;name&gt;/server/config/database/connectionUrl&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The JDBC URL that will be used to connect to the external database.  For example, &quot;jdbc:mysql://localhost:3406/cs030101?continueBatchOnError=false&amp;amp;useUnicode=true&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Connection URL&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server75&lt;/id&gt;
        &lt;name&gt;/server/config/database/databaseCatalog&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The database catalog that contains the Composite schema. It may be blank if the database does not support catalogs.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Catalog Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server76&lt;/id&gt;
        &lt;name&gt;/server/config/database/databaseHost&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The hostname where the database is located.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Host&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server77&lt;/id&gt;
        &lt;name&gt;/server/config/database/databaseName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The name of the database within the database.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server78&lt;/id&gt;
        &lt;name&gt;/server/config/database/databasePassword&lt;/name&gt;
        &lt;type&gt;PASSWORD_STRING&lt;/type&gt;
        &lt;annotation&gt;The database password.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Password&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server79&lt;/id&gt;
        &lt;name&gt;/server/config/database/databasePort&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The port where the database is located.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Port&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server80&lt;/id&gt;
        &lt;name&gt;/server/config/database/databaseSchema&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The database schema that contains the Composite schema. It may be blank if the database does not support schemas.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Schema Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server81&lt;/id&gt;
        &lt;name&gt;/server/config/database/databaseUser&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The database user name.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database User Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server82&lt;/id&gt;
        &lt;name&gt;/server/config/database/driverClass&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The fully qualified class name of a JDBC compliant driver.  For example, &quot;com.mysql.jdbc.Driver&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Driver Class&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server83&lt;/id&gt;
        &lt;name&gt;/server/config/database/driverClassPath&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A semi-colon or colon separated list of jar files and directories.  For example, &quot;/tmp/oracle40.jar:/tmp&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Driver Class Path&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server84&lt;/id&gt;
        &lt;name&gt;/server/config/database/driverLibraryPath&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The system path for any third party libraries that are used by the driver to create connections to repository database.&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Driver Library&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server85&lt;/id&gt;
        &lt;name&gt;/server/config/database/driverName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The name of the Composite datasource driver name. This is required for proper operation of the system tables.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Driver Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server86&lt;/id&gt;
        &lt;name&gt;/server/config/database/driverType&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The name of the Composite datasource driver type. This is required for proper operation of the system tables.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Driver Type&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server87&lt;/id&gt;
        &lt;name&gt;/server/config/database/loadExamples&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Indicates whether or not to load examples resources to a bundled repository.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Load Examples&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server88&lt;/id&gt;
        &lt;name&gt;/server/config/database/repositoryChanged&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Set to true if the repository configuration has been changed.  False if not.  The server sets this to false on startup after doing post-change processing.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Repository Changed&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server89&lt;/id&gt;
        &lt;name&gt;/server/config/database/repositoryClass&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The repository class name.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Repository Class&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server90&lt;/id&gt;
        &lt;name&gt;/server/config/database/schemaCreateScript&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Composite schema creation script. This script contains the SQL commands to create the Composite schema.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Schema Create Script&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server91&lt;/id&gt;
        &lt;name&gt;/server/config/database/schemaDropScript&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Composite schema drop script. This script contains the SQL commands to drop the Composite schema.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Schema Drop Script&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server92&lt;/id&gt;
        &lt;name&gt;/server/config/database/schemaInitializeScript&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Composite initialize script. This script contains the SQL commands to initialize the Composite tables.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Schema Initialize Script&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server93&lt;/id&gt;
        &lt;name&gt;/server/config/database/serviceName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Composite repository service name. This value is only used by Window platforms.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Service Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server94&lt;/id&gt;
        &lt;name&gt;/server/config/debug/DisableCardinalityHistogramCompression&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, CIS server will not redistribute cardinality histogram data.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable Cardinality Histogram Compression&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server95&lt;/id&gt;
        &lt;name&gt;/server/config/debug/NumberOfCardinalityHistogramBuckets&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;If specified, CIS will use user supplied value for number of cardinality histogram buckets; otherwise CIS will automatically choose the number of histogram buckets. Number of buckets should be specified as a comma separated list of TablePATH/columnname=value; value can only be integer value greater than 0. Example: /shared/examples/ds_inventory/products/col1=10, /shared/examples/ds_inventory/purchaseorders/col2=20 &amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&quot;&quot;&lt;/defaultValue&gt;
        &lt;displayName&gt;Number Of Cardinality Histogram Buckets&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server96&lt;/id&gt;
        &lt;name&gt;/server/config/debug/cache/coupleWritersWithReaders&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt; Useful in overriding the default parallel cache loading policy using writer threads that are decoupled from reader threads. &amp;lt;b&amp;gt;It should not be modified unless requested by Composite support. This setting will not be propagated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Couple Writers with Readers&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server97&lt;/id&gt;
        &lt;name&gt;/server/config/debug/cache/parallelLoadingLevel&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt; Useful in overriding the auto-determined number of reader threads used by parallel cache loading, when applicable. &amp;lt;b&amp;gt;It should not be modified unless requested by Composite support. This setting will not be propagated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;0&lt;/defaultValue&gt;
        &lt;displayName&gt;Parallel Loading Level&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server98&lt;/id&gt;
        &lt;name&gt;/server/config/debug/dbchannelTraceLevel&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;0&quot;. At level &quot;1&quot;, the server will log connection events. At level &quot;2&quot;, the server will log all but data transfer events. At level &quot;3&quot;, the server logs all events, including data. This setting should only be used when requested by Composite support to help in debugging an issue.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;0&lt;/defaultValue&gt;
        &lt;displayName&gt;JDBC/ODBC Trace Level&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server99&lt;/id&gt;
        &lt;name&gt;/server/config/debug/delayBetweenCacheGarbageCollectionJobs&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt; useful in reducing performance impact due to cache clear jobs; this parameter specifies delay, in seconds, between any two successive cache clear jobs. Set to 0 if no delay is required between successive cache clear jobs.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;0&lt;/defaultValue&gt;
        &lt;displayName&gt;Delay required between successive cache clear jobs in seconds&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server100&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableAllConnectionSafetyChecks&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;This mode is used for performance testing. When true, many parts of the system that log requests, change thread priories, validate connections, etc. are disabled. This mode should never be used in production.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Turbo Mode&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server101&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableCacheOrphanGarbageCollection&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt; Set to false by default. Disables cache garbage collection for orphan data (data whose cache keys does not exist in status table). This could help in reducing the load on cache target table due to cache refresh.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable Cache Orphan Garbage Collection&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server102&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableCacheRefreshes&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;. When &quot;true&quot;, the server will ignore all cache refreshes. This setting should only be used when requested by Composite support to help in debugging an issue.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable Cache Refreshes&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server103&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableRequestThrottling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Setting true will disable incoming request throttling.  Enabling this may result in CPU starvation if too many requests are processed simultaneously.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable incoming request throttling.&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server104&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableSSL&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;SSL for intra-cluster communication is disabled if set to true.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;SSL Disabled (Current)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server105&lt;/id&gt;
        &lt;name&gt;/server/config/debug/disableSSLOnServerRestart&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Disable SSL for intra-cluster communication if set to true.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This parameter should only be changed when the server is not in a cluster. &amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable SSL (On Server Restart)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server106&lt;/id&gt;
        &lt;name&gt;/server/config/debug/discardDSData&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, all data produced by data sources will be discarded and the data fetched will not be converted into composite value objects. This configuration is useful for performance testing to determine how much the value object conversion contributes to latency.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Discard Data Source Data&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server107&lt;/id&gt;
        &lt;name&gt;/server/config/debug/discardDbchannelData&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, all data produced by executing JDBC and ODBC requests will be not be sent to the client. This configuration is useful for performance testing to determine how much the JDBC/ODBC protocol contributes to latency.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Discard JDBC/ODBC Data&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server108&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableBulkDataLoading&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Setting to true will: &amp;lt;br&amp;gt; &amp;lt;br&amp;gt;
                       1. Allow INSERT/SELECT statements inserting rows into Netezza to use bulk loading methods. &amp;lt;br&amp;gt;
                       2. Allow procedures cached into Netezza to use bulk loading methods.
    &lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Bulk Data Loading&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server109&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDataShipProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce profiling information for each DataShip request in excess of what is normally produced.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;DataShip Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server110&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDebug&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce significant debugging output into its standard log file.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Output Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server111&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDebugForDataSources&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Setting true will log debug information for any datasource&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Output Enabled for Data Sources&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server112&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDebugForMemoryRequests&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Setting true will log debug information for managed memory allocation requests&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Output Enabled for Managed Memory Allocation Requests&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server113&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDebugForPools&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to false. When set to true, the server will periodically
    log the thread stack, which checks out connection from data source connection pool, into the cs_server_status.log&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Connection Pool Enabled for Data Sources&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server114&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableDetailedProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce detailed profiling information in excess of what is normally produced.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Detailed Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server115&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableGroupProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce profiling information for each CIS group.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Group Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server116&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableRequestDebugging&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, each incoming request will produce significant debugging output into separate log files.  This will have negative performance impact and may result in the creation of many log files.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Request Debug Output Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server117&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableRequestProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce profiling information for each request in excess of what is normally produced.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Request Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server118&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableSessionDebugging&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, each session will produce significant debugging output into separate log files.  This will have negative performance impact and may result in the creation of many log files.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Session Debug Output Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server119&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableSessionProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce profiling information for each session.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Session Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server120&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableSqlProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce aggregate profiling information for incoming sql statements.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;SQL Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server121&lt;/id&gt;
        &lt;name&gt;/server/config/debug/enableUserProfiling&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will produce profiling information for each user.  This will have negative performance impact.  This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;User Profiling Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server122&lt;/id&gt;
        &lt;name&gt;/server/config/debug/fakeJdbcData&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;When enabled, result sets from queries to a JDBC data source are cached in memory. The cached data is discarded when this mode is disabled. This mode is used to help debug performance issues. It should never be enabled on a production system.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Cache Result Sets from JDBC Data Sources&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server123&lt;/id&gt;
        &lt;name&gt;/server/config/debug/keepDataShipTempFileTables&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the server will not delete temp table or file generated for each DataShip request. This option should only be enabled when requested by a support team member.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;DataShip Keep Temp File or Table&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server124&lt;/id&gt;
        &lt;name&gt;/server/config/debug/maxConcurrentCacheGarbageCollectionJobs&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt; Useful in controlling maximum number of cache clear jobs. This will not take effect if delayBetweenGarbageCollectionJobs is set. Set to -1 if there is no restriction on maximum number of concurrent cache clear jobs.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;-1&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum number of concurrent cache clear jobs&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server125&lt;/id&gt;
        &lt;name&gt;/server/config/debug/onlyDebugExceptions&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;true&quot;.  When &quot;true&quot;, the server will produce debugging output related to foreground request execution into its standard log file.  When &quot;false&quot;, the server will send such errors to the client without logging them.  It will only log internal errors.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Output Enabled for Request Failures&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server126&lt;/id&gt;
        &lt;name&gt;/server/config/debug/repositoryConnClosers&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot; and a &quot;Repository is closed&quot; exception occurs, the identity of the closing thread is also display. This setting should only be used when requested by Composite support to help in debugging an issue.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Display Closers of Repository Connections&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server127&lt;/id&gt;
        &lt;name&gt;/server/config/debug/serializeCache&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to false. When set to true, cache refresh will not start until all cache operations are finished.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Serialize Cache Operations&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server128&lt;/id&gt;
        &lt;name&gt;/server/config/debug/serverStatusLoggingPeriod&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Defaults to an hour. When enabled the server will periodically log various usage statistics into the cs_server_status.log. A value of 0 stops the server from logging this information.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;60&lt;/defaultValue&gt;
        &lt;displayName&gt;Server Status Logging Period&lt;/displayName&gt;
        &lt;unitName&gt;Minutes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server129&lt;/id&gt;
        &lt;name&gt;/server/config/debug/useFifoRepositoryCache&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Defaults to &quot;false&quot;.  When &quot;true&quot;, the repository cache will use a FIFO eviction algorithm.  This may result in more database hits.  This may also result in better repository performance once the cache is loaded.  When &apos;false&apos;, LRU is the algorithm.  Changing this value will have no effect until the next server restart.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;FIFO repository cache enabled.&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server130&lt;/id&gt;
        &lt;name&gt;/server/config/email/messageFromAddress&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;When sending out e-mail messages, the server uses this value in the &amp;lt;b&amp;gt;From Address&amp;lt;/b&amp;gt; field. The value must be of the format &amp;lt;i&amp;gt;username&amp;lt;/i&amp;gt;@&amp;lt;i&amp;gt;hostname&amp;lt;/i&amp;gt;.&lt;/annotation&gt;
        &lt;displayName&gt;From Address&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server131&lt;/id&gt;
        &lt;name&gt;/server/config/email/smtpAuthRequired&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;This setting should be enabled if the SMTP server requires authentication for sending emails. &lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;SMTP Authentication Required&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server132&lt;/id&gt;
        &lt;name&gt;/server/config/email/smtpAuthUserName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The user name is used for connecting to the SMTP server for sending emails.&lt;/annotation&gt;
        &lt;displayName&gt;SMTP Authentication User Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server133&lt;/id&gt;
        &lt;name&gt;/server/config/email/smtpAuthUserPassword&lt;/name&gt;
        &lt;type&gt;PASSWORD_STRING&lt;/type&gt;
        &lt;annotation&gt;The password is used for connecting to the SMTP server for sending emails.&lt;/annotation&gt;
        &lt;displayName&gt;SMTP Authentication User Password&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server134&lt;/id&gt;
        &lt;name&gt;/server/config/email/smtpHost&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;This setting contains the host name or IP address of the SMTP server that the Server uses when sending out e-mail.&lt;/annotation&gt;
        &lt;displayName&gt;SMTP Host Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server135&lt;/id&gt;
        &lt;name&gt;/server/config/email/smtpPort&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This setting contains the port number of the SMTP server that the Server uses when sending out e-mail.&lt;/annotation&gt;
        &lt;defaultValue&gt;25&lt;/defaultValue&gt;
        &lt;displayName&gt;SMTP Port&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server136&lt;/id&gt;
        &lt;name&gt;/server/config/files/configDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the Server configuration directory, relative to the installation directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;/conf/server&lt;/defaultValue&gt;
        &lt;displayName&gt;Configuration Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server137&lt;/id&gt;
        &lt;name&gt;/server/config/files/customAdapterDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the custom adapter directory, relative to the installation directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;/conf/adapters/custom&lt;/defaultValue&gt;
        &lt;displayName&gt;Custom Adapters Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server138&lt;/id&gt;
        &lt;name&gt;/server/config/files/customJarImportDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the directory where imported Java Jar files will be placed.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Custom Jar Import Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server139&lt;/id&gt;
        &lt;name&gt;/server/config/files/databaseInstallDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the Server database directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Database Installation Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server140&lt;/id&gt;
        &lt;name&gt;/server/config/files/javaSecurityPropertiesFile&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the Composite Server Java security properties file.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Java Security Properties File&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server141&lt;/id&gt;
        &lt;name&gt;/server/config/files/jreDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the read-only Java runtime (JRE) directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Read-only JRE Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server142&lt;/id&gt;
        &lt;name&gt;/server/config/files/logDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Directory location that the Server uses for log files.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Log Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server143&lt;/id&gt;
        &lt;name&gt;/server/config/files/logDirectoryOnServerRestart&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Directory location that the Server uses for log files.  Changing this value will not copy any log files to the new location.  If the directory does not exist or the server or monitor processes do not have permission to create files in the directory, the server may be unable to start.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server
restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Log Directory (On Server Restart)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server144&lt;/id&gt;
        &lt;name&gt;/server/config/files/logFiles&lt;/name&gt;
        &lt;type&gt;STRING_ARRAY&lt;/type&gt;
        &lt;annotation&gt;Names of the logging files for the server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Logging Files&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server145&lt;/id&gt;
        &lt;name&gt;/server/config/files/logFilesExtended&lt;/name&gt;
        &lt;type&gt;STRING_ARRAY&lt;/type&gt;
        &lt;annotation&gt;Names of the logging files plus additional log information that may be useful for diagnostics.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Logging Files and Extra Data&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server146&lt;/id&gt;
        &lt;name&gt;/server/config/files/serverAppsInstallDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the read-only Server directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Read-only Server Installation Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server147&lt;/id&gt;
        &lt;name&gt;/server/config/files/serverConfInstallDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the read/write Server directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Read/Write Server Installation Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server148&lt;/id&gt;
        &lt;name&gt;/server/config/files/serverDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Location of the Server directory relative to the installation.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;/apps/server&lt;/defaultValue&gt;
        &lt;displayName&gt;Server Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server149&lt;/id&gt;
        &lt;name&gt;/server/config/files/tempDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Directory location that the Server uses for caching and data processing.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Temp Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server150&lt;/id&gt;
        &lt;name&gt;/server/config/files/tempDirectoryOnServerRestart&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Directory location that the Server uses for caching and data processing.  Changing this value will not copy any temporary files to the new location.  This will cause any automatic file caches and gathered statistics information to appear to be cleared.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server
restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Temp Directory (On Server Restart)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server151&lt;/id&gt;
        &lt;name&gt;/server/config/hooks/enableHooks&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, then installed hooks will be applied to all incoming SQL and procedure requests.  This also disables all performance advantages from prepared statements.  If false, hooks are not used even if present, and prepared statements run normally.
The use of hooks is strongly discouraged in production systems as they can lead to performance and stability problems with the server depending on the logic in installed hooks.  You may be requested to disable hooks when contacting support.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Hooks&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server152&lt;/id&gt;
        &lt;name&gt;/server/config/hooks/preventDisablement&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, then runtime errors will not cause hooks to be disabled.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Prevent Disablement&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server153&lt;/id&gt;
        &lt;name&gt;/server/config/info/currentTime&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The current time and date on the server in XML Schema format.  The trailing &apos;Z&apos; indicates GMT time.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Current Time&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server154&lt;/id&gt;
        &lt;name&gt;/server/config/info/displayName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A user-editable name of server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Server Display Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server155&lt;/id&gt;
        &lt;name&gt;/server/config/info/hostIpAddress&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The IP address of the server&apos;s host.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Host IP Address&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server156&lt;/id&gt;
        &lt;name&gt;/server/config/info/hostName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The hostname of the server&apos;s host.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Host Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server157&lt;/id&gt;
        &lt;name&gt;/server/config/info/jreVmMode&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Java runtime (JRE) VM mode of operation. 32-bit or 64-bit.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;JRE VM Mode&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server158&lt;/id&gt;
        &lt;name&gt;/server/config/info/locale&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;This setting determines the default locale of the server.  Possible values are fr, en, and ru.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;en&lt;/defaultValue&gt;
        &lt;displayName&gt;Locale&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server159&lt;/id&gt;
        &lt;name&gt;/server/config/info/name&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software product name.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;CompositeSoftware&lt;/defaultValue&gt;
        &lt;displayName&gt;Product Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server160&lt;/id&gt;
        &lt;name&gt;/server/config/info/serverId&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Server ID is a unique identifier for a single installation. This is set once at installation when the server is first started.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Server ID is a read-only value set as a unique identifier for the installation.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Server ID&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server161&lt;/id&gt;
        &lt;name&gt;/server/config/info/serviceName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The Composite server service name. This value is only used by Window platforms.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Service Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server162&lt;/id&gt;
        &lt;name&gt;/server/config/info/timeZone&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The time zone the server is running in.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Time Zone&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server163&lt;/id&gt;
        &lt;name&gt;/server/config/info/version&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software version number of the server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Server Software Version Number&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server164&lt;/id&gt;
        &lt;name&gt;/server/config/info/versionDetail&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software version of the server, including information on current patch and hotfix level.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Server Software Version Detail&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server165&lt;/id&gt;
        &lt;name&gt;/server/config/info/versionFull&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software version of the server in number.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Server Software Version Full Number&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server166&lt;/id&gt;
        &lt;name&gt;/server/config/info/versionFullDetail&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software version of the server, including information on all patches and hotfixes applied.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Server Software Version Full Detail&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server167&lt;/id&gt;
        &lt;name&gt;/server/config/info/versionName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The software version of the server without patch and hotfix level information.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Server Software Version Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server168&lt;/id&gt;
        &lt;name&gt;/server/config/license/licenseFile&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Name of the license file, relative to the configuration directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;csw_license.xml&lt;/defaultValue&gt;
        &lt;displayName&gt;Licence File Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server169&lt;/id&gt;
        &lt;name&gt;/server/config/license/licenseInfo&lt;/name&gt;
        &lt;type&gt;BYTE_ARRAY&lt;/type&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server170&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/batchParallelizationThreshold&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Parallelize batch operations when the number of batches in a single operation exceeds this threshold. Setting it to a large number effectively disables it.&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;500&lt;/defaultValue&gt;
        &lt;displayName&gt;Batch Parallelization Threshold&lt;/displayName&gt;
        &lt;minValue&gt;2&lt;/minValue&gt;
        &lt;unitName&gt;batches&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server171&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/enable&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, a summary of all changes to server metadata will be logged to a file on disk. The log will include the full path of the resource that was modified, the name of the user that modified it, and the time at which it was last modified.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Change Log&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server172&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/fileName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Name of the metadata change log. The file will be created under the path specified by Log Directory.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;cs_server_metadata%g.log&lt;/defaultValue&gt;
        &lt;displayName&gt;Log File&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server173&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/maxFiles&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The maximum number of log files. The minimum value is 1.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;10&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Number Of Log Files&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server174&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/maxFilesOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The maximum number of log files. The minimum value is 1.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;10&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Number Of Log Files (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server175&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/maxKBytes&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The maximum size of all log files combined, in kilobytes. The minimum value is 64.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Size Of All Log Files&lt;/displayName&gt;
        &lt;minValue&gt;64&lt;/minValue&gt;
        &lt;unitName&gt;KBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server176&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/changeLog/maxKBytesOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The maximum size of all log files combined, in kilobytes. The minimum value is 64.
        &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Size Of All Log Files (On ServerRestart)&lt;/displayName&gt;
        &lt;minValue&gt;64&lt;/minValue&gt;
        &lt;unitName&gt;KBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server177&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/commitCacheSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The commit record cache holds the most often-used metadata commit records in order to reduce access to the internal database.  The commit record cache is stored in the &quot;Reserved Memory&quot; area.  The minimum size is 100 records and the default size is 1000 records.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;1000&lt;/defaultValue&gt;
        &lt;displayName&gt;Metadata Commit Cache Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;100&lt;/minValue&gt;
        &lt;unitName&gt;records&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server178&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/commitCacheSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The commit record cache holds the most often-used metadata commit records in order to reduce access to the internal database.  The commit record cache is stored in the &quot;Reserved Memory&quot; area.  The minimum size is 100 records and the default size is 1000 records.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;1000&lt;/defaultValue&gt;
        &lt;displayName&gt;Metadata Commit Cache Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;100&lt;/minValue&gt;
        &lt;unitName&gt;records&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server179&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/optimizeDatabaseOnStartup&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;If true, the metadata tables will be analyzed for fragmentation and automatically optimized if necessary each time the server is started. Table fragmentation is not common and rarely is severe enough to adversly impact server performance. If defragmentation is necessary then the server may take longer to restart. The default value for this option is false.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Optimize Database on Server Startup&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server180&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/privilegeCacheSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The privilege cache holds the most often-used privilege data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;Privilege Cache Access&quot; falls below 90% under normal stable workload.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50000&lt;/defaultValue&gt;
        &lt;displayName&gt;Privilege Cache Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;500&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server181&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/privilegeCacheSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The privilege cache holds the most often-used privilege data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;Privilege Cache Access&quot; falls below 90% under normal stable workload.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;50000&lt;/defaultValue&gt;
        &lt;displayName&gt;Privilege Cache Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;500&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server182&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/purgeThreshold&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This configuration setting determines how many metadata objects may be deleted before the metadata repository is garbage collected.  The minimum value is 1 object and the default value is 2000 objects.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;Purge Threshold (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;records&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server183&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/purgeThresholdOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This configuration setting determines how many metadata objects may be deleted before the metadata repository is garbage collected.  The minimum value is 1 object and the default value is 2000 objects.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;Purge Threshold (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;records&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server184&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/relationshipCacheSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The relationship cache holds the most often-used relationship data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;Relationship Cache Access&quot; falls below 90% under normal stable workload.
      &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;100000&lt;/defaultValue&gt;
        &lt;displayName&gt;Relationship Cache Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1000&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server185&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/relationshipCacheSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The relationship cache holds the most often-used relationship data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;Relationship Cache Access&quot; falls below 90% under normal stable workload.
  &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
      &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;100000&lt;/defaultValue&gt;
        &lt;displayName&gt;Relationship Cache Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1000&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server186&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/resourceCacheSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The metadata cache holds the most often-used metadata objects in memory in order to reduce access to the internal database.  Server performance can improve with a larger cache size. However, increasing the size of the cache will reduce the amount of memory available for query processing. The metadata cache is stored in the &quot;Managed Memory&quot; area and will never grow larger than 25% of the total amount of managed memory, regardless of how large the cache size is set. The minimum size is 500 objects and the default size is 10000 objects.  Please check the &quot;Repository Cache Access&quot; statistic
in the Server Overview console.  Consider increasing this cache if the &quot;Repository Cache Access&quot; falls below 90% under normal stable workload.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;100000&lt;/defaultValue&gt;
        &lt;displayName&gt;Metadata Cache Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;1000&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server187&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/resourceCacheSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The metadata cache holds the most often-used metadata objects in memory in order to reduce access to the internal database.  Server performance can improve with a larger cache size. However, increasing the size of the cache will reduce the amount of memory available for query processing. The metadata cache is stored in the &quot;Managed Memory&quot; area and will never grow larger than 25% of the total amount of managed memory, regardless of how large the cache size is set. The minimum size is 500 objects and the default size is 10000 objects.  Please check the &quot;Repository Cache Access&quot; statistic
in the Server Overview console.  Consider increasing this cache if the &quot;Repository Cache Access&quot; falls below 90% under normal stable workload.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;100000&lt;/defaultValue&gt;
        &lt;displayName&gt;Metadata Cache Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;1000&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server188&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/useCorrectXmlNullableSettings&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;&amp;lt;b&amp;gt;true:&amp;lt;/b&amp;gt; When converting data types from native XML to CIS relational, only nillable XML elements will be marked as nullable.
      &amp;lt;p&amp;gt;&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;false:&amp;lt;/b&amp;gt; When converting data types from native XML to CIS relational, all generated elements will be marked as nullable.

      &amp;lt;p&amp;gt;&amp;lt;p&amp;gt;This setting allows transformations to use XML data types with expectations set correctly for nullability and should cause fewer warnings to appear due to nullable vs. not-nullable mismatches.  This setting defaults to false to conform with the behavior of older versions of the CIS.  This setting only affects the nullability of newly created types that are derived from XML.&amp;lt;/p&amp;gt;
      &amp;lt;p&amp;gt;&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Use Correct XML Nullable Settings&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server189&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/userCacheSize&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The user cache holds the most often-used user data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;User Cache Access&quot; falls below 90% under normal stable workload.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;User Cache Size (Current)&lt;/displayName&gt;
        &lt;minValue&gt;100&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server190&lt;/id&gt;
        &lt;name&gt;/server/config/metadata/userCacheSizeOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The user cache holds the most often-used user data in memory in order to reduce access to the database. Consider increasing this cache if the &quot;User Cache Access&quot; falls below 90% under normal stable workload.
&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;2000&lt;/defaultValue&gt;
        &lt;displayName&gt;User Cache Size (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;100&lt;/minValue&gt;
        &lt;unitName&gt;objects&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server191&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/pingInterval&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Ping frequency specified in seconds to send out server pings.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;120&lt;/defaultValue&gt;
        &lt;displayName&gt;Monitor Ping Interval&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server192&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/pingTimeout&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Timeout value in seconds used to determine that a server ping has failed.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;120&lt;/defaultValue&gt;
        &lt;displayName&gt;Monitor Ping Timeout&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server193&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/port&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Port that the monitor process receives requests from the Studio.  This port is always set to 6 ports higher than the server&apos;s web services port.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is a read-only value that indicates the current server state.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Monitor Port&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server194&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/serverIgnoreSignals&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable the option to ignore operating system signals for the JVM used by the Composite Server.  false: disable the option to ignore operating system signals for the JVM used by the Composite Server.&amp;lt;bg&amp;gt;

&amp;lt;p&amp;gt;This setting is useful for Windows systems that require the Composite Server process to not terminate during a Windows logoff event. It uses the -Xrs Sun JVM flag to provide this functionality.  Please refer to http://java.sun.com/j2se/1.5.0/docs/tooldocs/windows/java.html for further details.&amp;lt;/p&amp;gt;
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Server Ignore Signals&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server195&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/serverIgnoreSignalsOnMonitorRestart&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable the option to ignore operating system signals for the JVM used by the Composite Server.  false: disable the option to ignore operating system signals for the JVM used by the Composite Server.&amp;lt;bg&amp;gt;

&amp;lt;p&amp;gt;This setting is useful for Windows systems that require the Composite Server process to not terminate during a Windows logoff event. It uses the -Xrs Sun JVM flag to provide this functionality.  Please refer to http://java.sun.com/j2se/1.5.0/docs/tooldocs/windows/java.html for further details.&amp;lt;/p&amp;gt;

&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next monitor restart.&amp;lt;/b&amp;gt;
    &lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Server Ignore Signals(On Monitor Restart)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server196&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/showMessageBoxOnError&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable debugging for a fatal error encountered in the Composite Server JVM.  false: disable debugging for a fatal error encountered in the Composite Server JVM.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Show Message Box On Error&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server197&lt;/id&gt;
        &lt;name&gt;/server/config/monitor/showMessageBoxOnErrorOnMonitorRestart&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable debugging for a fatal error encountered in the Composite Server JVM.  false: disable debugging for a fatal error encountered in the Composite Server JVM.&amp;lt;bg&amp;gt;

&amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next monitor restart.&amp;lt;/b&amp;gt;
    &lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Show Message Box On Error(On Monitor Restart)&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server198&lt;/id&gt;
        &lt;name&gt;/server/config/net/disableHttpsHostnameVerification&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Disable hostname verification when connecting to servers using HTTPS.  Use with caution. Disabling hostname verification for HTTPS connections weakens the security of the connection.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Disable HTTPS Hostname Verification&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server199&lt;/id&gt;
        &lt;name&gt;/server/config/net/hostname&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The hostname of the server.
    &amp;lt;p&amp;gt;The network host name defaults to the local computer network name.&amp;lt;p&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Hostname&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server200&lt;/id&gt;
        &lt;name&gt;/server/config/net/nioConnectorMaxIdleTime&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The maximum amount of time in seconds for an idle NIO connection to wait for a request or read before the HTTP/S connection closes.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value does not take effect until server restart.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;200&lt;/defaultValue&gt;
        &lt;displayName&gt;NIO Connector Max Idle Time (sec)&lt;/displayName&gt;
        &lt;minValue&gt;60&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server201&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/ftp/enableProxy&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Use proxy server for FTP connections.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Use Proxy Server&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server202&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/ftp/proxyExclusions&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A list of hosts which should be connected to directly. Connections to these hosts will not be routed through the proxy server. The value can be a list of hosts, each seperated by a &apos;,&apos;, and in addition a wildcard character &apos;*&apos; can be used for matching. For example: &quot;localhost, *.composite.com, 10.1.1.18, 192.168.*.*&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;localhost&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Exclusion List&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server203&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/ftp/proxyHost&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The host name or IP address of the FTP Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Host&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server204&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/ftp/proxyPort&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The port used to communicate with the FTP Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;21&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Port&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server205&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/enableProxy&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Use proxy server for HTTP connections.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Use Proxy Server&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server206&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/proxyExclusions&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A list of hosts which should be connected to directly. Connections to these hosts will not be routed through the proxy server. The value can be a list of hosts, each seperated by a &apos;,&apos;, and in addition a wildcard character &apos;*&apos; can be used for matching. For example: &quot;localhost, *.composite.com, 10.1.1.18, 192.168.*.*&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;localhost&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Exclusion List&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server207&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/proxyHost&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The host name or IP address of the HTTP Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Host&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server208&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/proxyPassword&lt;/name&gt;
        &lt;type&gt;PASSWORD_STRING&lt;/type&gt;
        &lt;annotation&gt;The password used to authenticate to the proxy server. Leave it blank if no authentication is required. The value of the password stored on the server is encrypted, therefore the length of the password that is displayed is not necessarily the same length as the unencrypted password.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Proxy Password&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server209&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/proxyPort&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The port used to communicate with the HTTP Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;8080&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Port&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server210&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/http/proxyUsername&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The username used to authenticate to the proxy server. Leave it empty if no authentication is required.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Proxy Username&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server211&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/enableProxy&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Use proxy server for HTTPS connections.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Use Proxy Server&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server212&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/proxyExclusions&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;A list of hosts which should be connected to directly. Connections to these hosts will not be routed through the proxy server. The value can be a list of hosts, each seperated by a &apos;,&apos;, and in addition a wildcard character &apos;*&apos; can be used for matching. For example: &quot;localhost, *.composite.com, 10.1.1.18, 192.168.*.*&quot;.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;localhost&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Exclusion List&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server213&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/proxyHost&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The host name or IP address of the HTTPS Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Host&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server214&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/proxyPassword&lt;/name&gt;
        &lt;type&gt;PASSWORD_STRING&lt;/type&gt;
        &lt;annotation&gt;The password used to authenticate to the proxy server. Leave it blank if no authentication is required. The value of the password stored on the server is encrypted, therefore the length of the password that is displayed is not necessarily the same length as the unencrypted password.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Proxy Password&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server215&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/proxyPort&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The port used to communicate with the HTTPS Proxy server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;443&lt;/defaultValue&gt;
        &lt;displayName&gt;Proxy Port&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server216&lt;/id&gt;
        &lt;name&gt;/server/config/net/proxy/https/proxyUsername&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The username used to authenticate to the proxy server. Leave it blank if no authentication is required.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;Proxy Username&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server217&lt;/id&gt;
        &lt;name&gt;/server/config/net/useBlockingIOConnectors&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;When Use Blocking IO Connectors is set to false (the default value), Composite Server uses NIO for communications with Composite applications over HTTP and SSL/HTTPS. When Use Blocking IO connectors is set to true, Composite Server, on server restart, uses Blocking IO connectors for communications with Composite applications over HTTP and SSL/HTTPS. &amp;lt;p&amp;gt;It is recommended to use Blocking IO connectors on platforms with known NIO connector problems. Composite Server restart is required for the change in the value of this setting to take effect. &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Use Blocking IO Connectors&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server218&lt;/id&gt;
        &lt;name&gt;/server/config/net/wsdlhostname&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The hostname used for web service addresses published in the WSDL file.
    &amp;lt;p&amp;gt;The network host name defaults to the local computer network name, but it may be changed to use another host name like a DNS to support use of a third party load balancer for distribution of web services requests.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;Use the &amp;lt;b&amp;gt;Apply&amp;lt;/b&amp;gt; button to change all current WSDL definition sets (except local system WSDL definition sets) with the new network host name.  This will reset the named location for web services in the Composite repository.&amp;lt;p&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;WSDL Hostname&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server219&lt;/id&gt;
        &lt;name&gt;/server/config/osgi/bundles/BundleUninstallPauseInMillis&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Composite Server pause time in milli seconds after an OSGi bundle is stopped and uninstalled when a composite project is undeployed.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;10&lt;/defaultValue&gt;
        &lt;displayName&gt;OSGi Bundle Uninstall Pause Time&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;MilliSeconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server220&lt;/id&gt;
        &lt;name&gt;/server/config/osgi/bundles/bundleDirectory&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Directory location that the Composite Server uses for loading OSGi bundles.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;displayName&gt;OSGi Bundle Directory&lt;/displayName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server221&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/AllowNTLM&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Whether or not to allow NTLM to be used for authentication; do not enable before configuring NTLM (see documentation for details).&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Allow NTLM Authentication&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server222&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/AthnFailureLimit&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;The number of failed login attempts before a user account is locked&lt;/annotation&gt;
        &lt;defaultValue&gt;50&lt;/defaultValue&gt;
        &lt;displayName&gt;Implicit Lock Threshold&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server223&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/EnableAthnLocking&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Turns on or off account locking due to authentication failure.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Implicit Account Locking&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server224&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/IPFiltering/Forbidden&lt;/name&gt;
        &lt;type&gt;LIST&lt;/type&gt;
        &lt;annotation&gt;IP Addresses and Address Patterns (e.g. 10.*.*.*) from which no user may authenticate.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;If no addresses or address patterns are specified, then any address is allowed.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;The address pattern *.*.*.* will be disregarded.&lt;/annotation&gt;
        &lt;displayName&gt;Forbidden IP Addresses&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server225&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/IPFiltering/Required&lt;/name&gt;
        &lt;type&gt;LIST&lt;/type&gt;
        &lt;annotation&gt;IP Addresses and Address Patterns (e.g. 10.*.*.*) from which a user may be authenticated.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;If no addresses or address patterns are specified, then no particular address is required.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;The address pattern *.*.*.* will be disregarded.&lt;/annotation&gt;
        &lt;displayName&gt;Required IP Addresses&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server226&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/NTLMDomain&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The external domain being used for NTLM authentication.  This is only ever required for non-Microsoft hosts. For Microsoft Windows hosts or SAMBA-based hosts that are configured to return usernames in &quot;DOMAIN\user&quot; format, use the Windows Domain Mapping setting instead of configuring this setting.&lt;/annotation&gt;
        &lt;displayName&gt;NTLM External Domain&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server227&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/TolerateUnusedSchemes&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Valid values are &quot;WARN&quot;, &quot;IGNORE&quot;, &quot;ERROR&quot;.&lt;/annotation&gt;
        &lt;defaultValue&gt;WARN&lt;/defaultValue&gt;
        &lt;displayName&gt;Tolerate Unused HTTP Authentication Schemes&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server228&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/WindowsDomainMapping&lt;/name&gt;
        &lt;type&gt;MAP&lt;/type&gt;
        &lt;annotation&gt;The mapping from the reported windows domain of an authenticated user to the name of the corresponding external domain as it is defined in the Composite Server.  The reported domain is the windows domain as it would be specified in a SAM name such as &quot;DOMAINNAME\username&quot;.&lt;/annotation&gt;
        &lt;displayName&gt;Windows Domain Mapping&lt;/displayName&gt;
        &lt;unitName&gt;Key-Value Map&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server229&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/AllowKerberos&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;This property enables or disables Kerberos authentication.  Do not enable Kerberos authentication prior to configuration or authentication failures may occur.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Allow Kerberos Authentication&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server230&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/Debug&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;When set to True, the JDK&apos;s Kerberos implementation will log output messages to the cs_server.out file in the logs directory.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     It&apos;s JGSS Property.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Debug Output Kerberos Authentication Enabled&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server231&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/KeyTab&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The KeyTab file contains pairs of Kerberos principals and encrypted keys used to authenticate the Composite Server to the Kerberos KDC.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;To enable CIS authentication with Kerberos:&amp;lt;p&amp;gt;Generate a KeyTab file using the Kerberos ktpass utility and place that file in a directory that is locally accessible to CIS.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;Enter the directory path and name of the KeyTab file generated for CIS usage as the value of this property.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     It&apos;s JGSS Property.&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;KeyTab File&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server232&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/Krb5Conf&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;The kerberos configuration file contains Kerberos configuration information, including the locations of KDCs and admin servers for the Kerberos realms of interest, defaults for the current realm and for Kerberos applications, and mappings of hostnames onto Kerberos realms.  On Windows, this is usually c:\WINDOWS\krb.ini; on Unix, it is usually /etc/krb.conf.  &amp;lt;p&amp;gt;&amp;lt;p&amp;gt;Changes to this value do not take effect until server restart.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     It&apos;s JGSS Property.&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Kerberos Configuration File&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server233&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/Native&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;This property switch Kerberos facilities between SSPI and JGSS.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     &quot;True&quot;: use SSPI facility.&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;      &quot;False&quot;: use JGSS facility.  &lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Native&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server234&lt;/id&gt;
        &lt;name&gt;/server/config/security/authentication/kerberos/PrincipalName&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Enter the Service Principal Name registered by the Kerberos KDC. The entry will have a format like the following:&amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     HTTP/hostname@DOMAIN &amp;lt;p&amp;gt;&amp;lt;p&amp;gt;     It&apos;s JGSS Property.&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Required Principal Name&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server235&lt;/id&gt;
        &lt;name&gt;/server/config/security/enableAnonymousLogin&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable anonymous user login in the Composite domain.  false: disable anonymous user login in the Composite domain.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Anonymous Login&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server236&lt;/id&gt;
        &lt;name&gt;/server/config/security/enableDynamicLogin&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable Dynamic domain.  false: disable Dynamic domain.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Dynamic Domain Login&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server237&lt;/id&gt;
        &lt;name&gt;/server/config/security/enablePrivilegeLogging&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;if true, privilege changes to otherwise unchanged resources will be written to the metadata log&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable Privilege Logging&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server238&lt;/id&gt;
        &lt;name&gt;/server/config/security/enableRowBasedSecurity&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;determines whether Row Based Security is on or off&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Row Based Security Enabled&lt;/displayName&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server239&lt;/id&gt;
        &lt;name&gt;/server/config/security/export/ownerOnly&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Only allow the owner of a resource to export it.  &amp;lt;p&amp;gt;Note: The admin user may always perform a package export regardless of whether this configuration option is set.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Owner Export Only&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server240&lt;/id&gt;
        &lt;name&gt;/server/config/security/export/requiredGroupMembership&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Restrict package export to members of the group specified here. &amp;lt;p&amp;gt;Note: The admin user may always perform a package export regardless of whether this configuration option is set.&lt;/annotation&gt;
        &lt;defaultValue&gt;&lt;/defaultValue&gt;
        &lt;displayName&gt;Required Group Membership&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server241&lt;/id&gt;
        &lt;name&gt;/server/config/security/export/stripPasswords&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Strip passwords from exported resources.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Strip Passwords&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server242&lt;/id&gt;
        &lt;name&gt;/server/config/security/export/writersOnly&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;Only allow users with write privileges to a resource to export it. &amp;lt;p&amp;gt;Note: The admin user may always perform a package export regardless of whether this configuration option is set.&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Writers Only&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server243&lt;/id&gt;
        &lt;name&gt;/server/config/support/uploadURL&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;URL used by Send/Save Logs for Support to upload files to Composite Support&lt;/annotation&gt;
        &lt;defaultValue&gt;https://support.compositesw.com/logs/upload.php&lt;/defaultValue&gt;
        &lt;displayName&gt;URL to Upload to Composite&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server244&lt;/id&gt;
        &lt;name&gt;/server/config/transactions/maxParameterSizeLogged&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This value specifies an upper limit on the size for a parameter value to be added the transaction log.  Parameter values that exceed this size will be truncated before being added to the transaction log.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;64&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Parameter Size Logged&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;KBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server245&lt;/id&gt;
        &lt;name&gt;/server/config/transactions/maxRequestDepth&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This value specifies an upper limit on the depth of the request stack in a transaction.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;30&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Request Depth&lt;/displayName&gt;
        &lt;minValue&gt;10&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server246&lt;/id&gt;
        &lt;name&gt;/server/config/transactions/maxTotalSizeLogged&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;This value specifies an upper limit on the total size for a work unit be added the transaction log.  Statements that exceed this size will be truncated before being added to the transaction log.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;1024&lt;/defaultValue&gt;
        &lt;displayName&gt;Maximum Total Size Logged&lt;/displayName&gt;
        &lt;minValue&gt;0&lt;/minValue&gt;
        &lt;unitName&gt;KBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server247&lt;/id&gt;
        &lt;name&gt;/server/config/xslt/enablePersistent&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable persistent when executing XSLT transformation. It is for larget document transformation.  false: disable persistent when executing XSLT transformation.
      &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;This value is locally defined.  It will not be altered when restoring a backup and will not be replicated in a cluster.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;false&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable XSLT persistent&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server248&lt;/id&gt;
        &lt;name&gt;/server/event/conditions/storage/storageLowCheckPeriod&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Determines how often the storage threshold is checked for the Composite Server.&lt;/annotation&gt;
        &lt;defaultValue&gt;120&lt;/defaultValue&gt;
        &lt;displayName&gt;Storage Size Check Period&lt;/displayName&gt;
        &lt;minValue&gt;60&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_ONLY&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server249&lt;/id&gt;
        &lt;name&gt;/server/event/conditions/storage/storageLowCheckPeriodOnServerRestart&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Determines how often the storage threshold is checked for the Composite Server.
    &amp;lt;p&amp;gt;&amp;lt;b&amp;gt;Changing this value will have no effect until the next server restart.&amp;lt;/b&amp;gt;&lt;/annotation&gt;
        &lt;defaultValue&gt;120&lt;/defaultValue&gt;
        &lt;displayName&gt;Storage Size Check Period (On Server Restart)&lt;/displayName&gt;
        &lt;minValue&gt;60&lt;/minValue&gt;
        &lt;unitName&gt;seconds&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server250&lt;/id&gt;
        &lt;name&gt;/server/event/conditions/storage/storageLowCritical&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Determines when a storage critical warning event will occur.&lt;/annotation&gt;
        &lt;defaultValue&gt;100&lt;/defaultValue&gt;
        &lt;displayName&gt;Storage Critical Size&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;MBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server251&lt;/id&gt;
        &lt;name&gt;/server/event/conditions/storage/storageLowWarning&lt;/name&gt;
        &lt;type&gt;INTEGER&lt;/type&gt;
        &lt;annotation&gt;Determines when a storage low warning event will occur.&lt;/annotation&gt;
        &lt;defaultValue&gt;500&lt;/defaultValue&gt;
        &lt;displayName&gt;Storage Low Size&lt;/displayName&gt;
        &lt;minValue&gt;1&lt;/minValue&gt;
        &lt;unitName&gt;MBytes&lt;/unitName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server252&lt;/id&gt;
        &lt;name&gt;/server/event/enableEvents&lt;/name&gt;
        &lt;type&gt;BOOLEAN&lt;/type&gt;
        &lt;annotation&gt;true: enable event generation.  false: disable all event generation for the server.&lt;/annotation&gt;
        &lt;defaultValue&gt;true&lt;/defaultValue&gt;
        &lt;displayName&gt;Enable System Events&lt;/displayName&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server253&lt;/id&gt;
        &lt;name&gt;/server/event/generation/caches/cacheClear&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Use a comma to separate multiple choices. Multiple choices cannot
        include ALL or NONE.
        Currently supported event filters:
            &amp;lt;li&amp;gt;DB: event sent to database only.
            &amp;lt;li&amp;gt;LOG: event sent to log file only.
            &amp;lt;li&amp;gt;SNMP: event sent to SNMP processor only.
            &amp;lt;li&amp;gt;CUSTOM: event sent to custom event handler only.
            &amp;lt;li&amp;gt;ALL: event sent to database, log, SNMP processor, and custom event handler.
            &amp;lt;li&amp;gt;NONE: event ignored.&lt;/annotation&gt;
        &lt;defaultValue&gt;DB,LOG&lt;/defaultValue&gt;
        &lt;displayName&gt;Cache Clear&lt;/displayName&gt;
        &lt;pattern&gt;((?i)((DB|LOG|SNMP|CUSTOM)(,(DB|LOG|SNMP|CUSTOM))*)|ALL|NONE)&lt;/pattern&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server254&lt;/id&gt;
        &lt;name&gt;/server/event/generation/caches/cacheDisable&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Use a comma to separate multiple choices. Multiple choices cannot
        include ALL or NONE.
        Currently supported event filters:
            &amp;lt;li&amp;gt;DB: event sent to database only.
            &amp;lt;li&amp;gt;LOG: event sent to log file only.
            &amp;lt;li&amp;gt;SNMP: event sent to SNMP processor only.
            &amp;lt;li&amp;gt;CUSTOM: event sent to custom event handler only.
            &amp;lt;li&amp;gt;ALL: event sent to database, log, SNMP processor, and custom event handler.
            &amp;lt;li&amp;gt;NONE: event ignored.&lt;/annotation&gt;
        &lt;defaultValue&gt;DB,LOG&lt;/defaultValue&gt;
        &lt;displayName&gt;Cache Disable&lt;/displayName&gt;
        &lt;pattern&gt;((?i)((DB|LOG|SNMP|CUSTOM)(,(DB|LOG|SNMP|CUSTOM))*)|ALL|NONE)&lt;/pattern&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server255&lt;/id&gt;
        &lt;name&gt;/server/event/generation/caches/cacheEnable&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Use a comma to separate multiple choices. Multiple choices cannot
        include ALL or NONE.
        Currently supported event filters:
            &amp;lt;li&amp;gt;DB: event sent to database only.
            &amp;lt;li&amp;gt;LOG: event sent to log file only.
            &amp;lt;li&amp;gt;SNMP: event sent to SNMP processor only.
            &amp;lt;li&amp;gt;CUSTOM: event sent to custom event handler only.
            &amp;lt;li&amp;gt;ALL: event sent to database, log, SNMP processor, and custom event handler.
            &amp;lt;li&amp;gt;NONE: event ignored.&lt;/annotation&gt;
        &lt;defaultValue&gt;DB,LOG&lt;/defaultValue&gt;
        &lt;displayName&gt;Cache Enable&lt;/displayName&gt;
        &lt;pattern&gt;((?i)((DB|LOG|SNMP|CUSTOM)(,(DB|LOG|SNMP|CUSTOM))*)|ALL|NONE)&lt;/pattern&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
        &lt;id&gt;server256&lt;/id&gt;
        &lt;name&gt;/server/event/generation/caches/cacheRefreshEnd&lt;/name&gt;
        &lt;type&gt;STRING&lt;/type&gt;
        &lt;annotation&gt;Use a comma to separate multiple choices. Multiple choices cannot
