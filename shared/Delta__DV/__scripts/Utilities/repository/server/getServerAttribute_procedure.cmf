        &lt;displayName&gt;Maximum Columns for Multicolumn Key&lt;/displayName&gt;
        &lt;maxValue&gt;10&lt;/maxValue&gt;
        &lt;minValue&gt;2&lt;/minValue&gt;
        &lt;updateRule&gt;READ_WRITE&lt;/updateRule&gt;
    &lt;/serverAttributeDef&gt;
    &lt;serverAttributeDef&gt;
