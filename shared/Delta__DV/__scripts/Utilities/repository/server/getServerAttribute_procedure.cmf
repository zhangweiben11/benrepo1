        &lt;defaultValue&gt;1000&lt;/defaultValue&gt;
        &lt;displayName&gt;Purge Frequency&lt;/displayName&gt;
        &lt;maxValue&gt;5000&lt;/maxValue&gt;
        &lt;minValue&gt;100&lt;/minValue&gt;
