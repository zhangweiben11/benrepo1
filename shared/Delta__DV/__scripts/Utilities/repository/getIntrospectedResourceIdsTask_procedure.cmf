PROCEDURE getIntrospectedResourceIdsTask(
    IN dsPath &#x9;&#x9;&#x9;VARCHAR(4096),
    OUT taskId &#x9;&#x9;&#x9;LONGVARCHAR,
    OUT totalResults &#x9;INTEGER
)
BEGIN
    DECLARE moduleName &#x9;&#x9;VARCHAR(255) DEFAULT &apos;getIntrospectedResourceIdsTask&apos;;
    DECLARE resourceExists &#x9;BOOLEAN;
    DECLARE requestXML &#x9;&#x9;LONGVARCHAR;

    IF (dsPath IS NULL) THEN
        RAISE System.IllegalArgumentException VALUE moduleName || &apos;: The input &quot;dsPath&quot; may not be NULL.&apos;;
    END IF;

    CALL /lib/resource/ResourceExists (
        dsPath,
        /lib/resource/ResourceDefs.RESOURCE_TYPE_DATA_SOURCE,
        NULL,
        resourceExists
    );
    IF (NOT resourceExists) THEN
        RAISE System.NotFoundException VALUE moduleName || &apos;: The input data source, &quot;&apos; || dsPath || &apos;&quot;, does not exist or is not a data source.&apos;;
    END IF;

    SET requestXML = 
&apos;&lt;resource:getIntrospectedResourceIdsTask xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
    &lt;resource:path&gt;&apos; || CAST(XMLTEXT(dsPath) AS VARCHAR(2147483647)) || &apos;&lt;/resource:path&gt;
&lt;/resource:getIntrospectedResourceIdsTask&gt;
&apos;;

    myLoop:
    FOR r AS
        SELECT 
            taskId,
            CAST(totalResults AS INTEGER) totalResults
        FROM      
            /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/lowerLevelProcedures/getIntrospectedResourceIdsTaskXSLT(requestXML)
     DO
         SET taskId = r.taskId;
         SET totalResults = r.totalResults;
         LEAVE myLoop;
     END FOR;
END</attribute>
