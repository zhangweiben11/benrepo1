<metadata name="changeResourceOwner" path="/shared/Delta_DV/_scripts/Utilities/repository/changeResourceOwner" type="PROCEDURE" subtype="SQL_SCRIPT_PROCEDURE" changeToken="0">
  <annotation><![CDATA[changeResourceOwner:

