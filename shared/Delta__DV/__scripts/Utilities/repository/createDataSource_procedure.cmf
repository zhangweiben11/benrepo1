PROCEDURE createDataSource(
    IN dataSourcePath                 /lib/resource/ResourceDefs.ResourcePath, 
    IN dataSourceName                 /lib/resource/ResourceDefs.ResourceName,
    IN dataSourceType                 /lib/resource/ResourceDefs.ResourceType,
    OUT success                       BIT,
    OUT createDataSourceResponse      XML,
    OUT faultResponse                 XML
)
BEGIN
    DECLARE moduleName  VARCHAR DEFAULT &apos;createDataSource&apos;;
    DECLARE debug       CHAR(1) DEFAULT /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug;
&#x9;DECLARE dsPath&#x9;&#x9;/lib/resource/ResourceDefs.ResourcePath DEFAULT dataSourcePath;
&#x9;DECLARE dsName&#x9;&#x9;/lib/resource/ResourceDefs.ResourceName DEFAULT dataSourceName;
&#x9;DECLARE pos&#x9;&#x9;&#x9;INTEGER;
    DECLARE createInput LONGVARCHAR;

&#x9;IF (dsName IS NULL OR LENGTH(dsName) = 0) THEN
&#x9;&#x9;SET pos = INSTR(dsPath, &apos;/&apos;, -1, 1);
&#x9;&#x9;IF (pos &gt; 0) THEN
&#x9;&#x9;&#x9;SET dsName = SUBSTRING(dsPath, pos+1);
&#x9;&#x9;&#x9;SET dsPath = SUBSTRING(dsPath, 1, pos-1);
&#x9;&#x9;END IF;
&#x9;END IF;

    SET success = 1;
    SET createInput =
&apos;&lt;resource:createDataSource 
    xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; 
    xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; 
    xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
    &lt;resource:path&gt;&apos; || CAST(XMLTEXT(dsPath) AS LONGVARCHAR) || &apos;&lt;/resource:path&gt;
    &lt;resource:name&gt;&apos; || CAST(XMLTEXT(dsName) AS LONGVARCHAR) || &apos;&lt;/resource:name&gt;
    &lt;resource:detail&gt;FULL&lt;/resource:detail&gt;
    &lt;resource:dataSourceType&gt;&apos; || CAST(XMLTEXT(dataSourceType) AS LONGVARCHAR) || &apos;&lt;/resource:dataSourceType&gt;
&lt;/resource:createDataSource&gt;&apos;;

    CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug, CHR(10)||createInput);
    CALL /services/webservices/system/admin/resource/operations/createDataSource(createInput, createDataSourceResponse, faultResponse) ;

    IF (faultResponse IS NOT NULL) THEN
        SET success = 0;
    END IF;
END</attribute>
