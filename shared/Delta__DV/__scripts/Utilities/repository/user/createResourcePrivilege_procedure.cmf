                            &lt;ns1:name&gt;&apos; || CAST(XMLTEXT(name) AS LONGVARCHAR) || &apos;&lt;/ns1:name&gt;
                            &lt;ns1:nameType&gt;&apos; || nameType || &apos;&lt;/ns1:nameType&gt;
                            &lt;ns1:privs&gt;&apos; || REPLACE(&quot;privileges&quot;,&apos;|&apos;,&apos; &apos;) || &apos;&lt;/ns1:privs&gt;
                         &lt;/ns1:privilege&gt;
                       &lt;/ns1:privileges&gt;
                     &lt;/ns1:privilegeEntry&gt;
                   &lt;/ns1:privilegeEntries&gt;
                   &lt;ns1:mode&gt;SET_EXACTLY&lt;/ns1:mode&gt;
                &lt;/ns1:updateResourcePrivileges&gt;&apos;;

&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName, debug, request);
&#x9;&#x9;END IF;

&#x9;&#x9;-- Invoke the API to update the resource privilege
&#x9;&#x9;CALL /services/webservices/system/admin/resource/operations/updateResourcePrivileges(request, response, fault);
&#x9;EXCEPTION                     
