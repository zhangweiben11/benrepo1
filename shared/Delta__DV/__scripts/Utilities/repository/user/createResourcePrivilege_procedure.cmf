PROCEDURE createResourcePrivilege(
&#x9;IN debug &#x9;&#x9;&#x9;&#x9;CHAR(1),-- e.g. Y or N
&#x9;IN owner&#x9;&#x9;&#x9;&#x9;VARCHAR(8192),&#x9;-- Owner should be set to the name of the user which should own all the objects
&#x9;IN ownerDomain&#x9;&#x9;&#x9;VARCHAR(8192),&#x9;-- Domain for owner - default is &apos;composite&apos;
&#x9;IN resourcePath&#x9;&#x9;&#x9;VARCHAR(4096),&#x9;-- full resource path to the composite resource
&#x9;IN resourceType &#x9;&#x9;VARCHAR,&#x9;&#x9;-- resource type e.g. CONTAINER, DATA_SOURCE, PROCEDURE, VIEW
&#x9;IN recurse &#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- 1=recurse through folders if the resource is a folder, 0=no recurse
&#x9;IN name&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR(8192),&#x9;-- name of the user or group to assign the privilege
&#x9;IN domainName&#x9;&#x9;&#x9;VARCHAR(8192),&#x9;-- composite domain
&#x9;IN nameType&#x9;&#x9;&#x9;&#x9;VARCHAR(8192),&#x9;-- USER or GROUP
&#x9;IN &quot;privileges&quot;&#x9;&#x9;&#x9;LONGVARCHAR&#x9;&#x9;-- space separated list of privileges: READ SELECT EXECUTE INSERT UPDATE DELETE
)
BEGIN
&#x9;DECLARE moduleName &#x9;&#x9;VARCHAR DEFAULT &apos;createResourcePrivilege&apos;;
&#x9;DECLARE request &#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE response &#x9;&#x9;XML;
&#x9;DECLARE fault &#x9;&#x9;&#x9;XML;
&#x9;DECLARE message &#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE padChar&#x9;&#x9;&#x9;CHAR(1) DEFAULT &apos; &apos;;
    DECLARE ex &#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;-- Step one: Display the existing privileges in debug mode only
&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
        CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES FOR &apos; || resourcePath);
&#x9;&#x9;SET message = &#x9;RPAD(&apos;domain&apos;,12,padChar) ||&apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;name&apos;,12,padChar) || &apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;nameType&apos;,10,padChar) || &apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;privs&apos;,60,padChar) || &apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;combinedPrivs&apos;,60,padChar) || &apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;inheritedPrivs&apos;,60,padChar);&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES:&apos; || message);
&#x9;&#x9;SET message = &#x9;RPAD(&apos;------------&apos;,12,padChar) ||&apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;------------&apos;,12,padChar) || &apos;  &apos;||
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(&apos;----------&apos;,10,padChar) || &apos;  &apos;||
