&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;RPAD(r.inheritedPrivs,60,padChar);&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
&#x9;&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug, &apos;EXISTING PRIVILEGES:&apos; || message);
&#x9;&#x9;END FOR;

        CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug, &apos;Assigning PRIVILEGES TO &apos; || resourcePath);
&#x9;END IF;

&#x9;BEGIN                  
&#x9;&#x9;-- Step two: Assign privileges to all resources
&#x9;&#x9;SET request =
               &apos;&lt;ns1:updateResourcePrivileges xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/resource&quot;
                     xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;
                     xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;
