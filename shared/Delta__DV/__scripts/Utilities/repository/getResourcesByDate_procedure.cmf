&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set smallestModifiedDateBigint = resourceVect[i].lastModifiedDateBigint;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set smallestModifiedDateRow = i;
