&#x9;end if;

&#x9;-- 3: Find all resources modified after the passed in resource date
&#x9;if (optionType = OPTION_MODIFIED_AFTER) then
&#x9;&#x9;-- A resource date must be provided
&#x9;&#x9;if (resourceDate is null) then
