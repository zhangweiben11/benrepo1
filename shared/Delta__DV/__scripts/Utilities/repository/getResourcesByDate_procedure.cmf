&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;end for;

&#x9;&#x9;set i = 1;
&#x9;&#x9;while (i &lt;= CARDINALITY(resourceVect)) do
&#x9;&#x9;&#x9;if (resourceVect[i] is not null) then
&#x9;&#x9;&#x9;&#x9;set resourceRec = resourceVect[i];
