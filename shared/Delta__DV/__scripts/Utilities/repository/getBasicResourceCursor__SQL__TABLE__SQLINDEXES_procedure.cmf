PROCEDURE getBasicResourceCursor_SQL_TABLE_SQLINDEXES(
    IN fullResourcePath &#x9;VARCHAR(4096),  
    IN resourceType &#x9;&#x9;VARCHAR, 
    OUT resourceCursor CURSOR (
        sqlIndexName &#x9;VARCHAR(255), 
        sqlIndexType &#x9;VARCHAR(255), 
        sqlIndexUnique &#x9;BIT, 
        sqlIndexColName VARCHAR(255), 
        sqlIndexColOrder VARCHAR(255)
        )
    )
BEGIN
&#x9;DECLARE resourceResponse XML;
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/lowerLevelProcedures/getBasicResourceXML(fullResourcePath, resourceType, resourceResponse);
        OPEN resourceCursor FOR 
            SELECT * FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/lowerLevelProcedures/getBasicResourceXSLT_SQL_TABLE_SQLINDEXES(resourceResponse);
END</attribute>
