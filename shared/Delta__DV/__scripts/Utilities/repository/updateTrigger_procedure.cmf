PROCEDURE updateTrigger(
&#x9;IN fullResourcePath&#x9;&#x9;LONGVARCHAR,&#x9;&#x9;-- Full path to the trigger resource.
&#x9;IN enabled&#x9;&#x9;&#x9;&#x9;BIT,&#x9;&#x9;&#x9;&#x9;-- 1=enabled, 0=disabled
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- When only performing the enable or disable for a trigger, leave conditionType and actionType null. 
&#x9;-- Condition Type: [TIMER|JMS|SYSTEM_EVENT|USER_DEFINED]
&#x9;IN conditionType&#x9;&#x9;VARCHAR, &#x9;&#x9;&#x9;-- NULL or one of [TIMER|JMS|SYSTEM_EVENT|USER_DEFINED] determines the trigger condition.
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- When NULL, then only perform the enable or disable of the trigger and not other updates.
&#x9;-- Action Type: [PROCEDURE|STATISTICS|REINTROSPECT|EMAIL]
&#x9;IN actionType&#x9;&#x9;&#x9;VARCHAR,&#x9;&#x9;&#x9;-- NOT NULL and [PROCEDURE|STATISTICS|REINTROSPECT|EMAIL] - One of these determines what type of action to take.
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- When NULL, then only perform the enable or disable of the trigger and not other updates.
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- When conditionType is NOT NULL, there must be an actionType.
&#x9;IN triggerAttrVect&#x9;&#x9;VECTOR(TriggerType),-- Name/Value pair list containing only the necessary &quot;Condition Type&quot; and &quot;Action Type&quot; attributes.

&#x9;IN attributeVect&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/definitions/RepositoryDefinitions.AttributeVectorType,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- The Attribute Vectory Type is a vector of type AttributeType as follows:
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- attrName&#x9;&#x9;&#x9;VARCHAR(255),
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- attrType&#x9;&#x9;&#x9;VARCHAR(255),
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- attrValue&#x9;&#x9;VARCHAR(255)
&#x9;OUT success&#x9;&#x9;&#x9;&#x9;BIT,
&#x9;OUT createResponse &#x9;&#x9;XML,
&#x9;OUT faultResponse&#x9;&#x9;XML
)
BEGIN
    DECLARE PROC_NAME   &#x9;&#x9;&#x9;CONSTANT VARCHAR DEFAULT &apos;repository/updateTrigger&apos;;
&#x9;DECLARE debug&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1) DEFAULT /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug;
    DECLARE PUBLIC TYPE TriggerType ROW (
        trName &#x9;&#x9;VARCHAR, 
        trValue &#x9;LONGVARCHAR
    );
-- CONDITION TYPE ATTRIBUTES:
&#x9;-- When conditionType=TIMER set these variables
&#x9;DECLARE timerStartTime&#x9;&#x9;&#x9;&#x9;&#x9;TIMESTAMP; &#x9;-- NULL=mode=NONE or the timestamp the trigger should start.    Format = YYYY-MM-DD HH24:MM:SS
&#x9;DECLARE timerPeriod&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR; &#x9;-- One of [MINUTE|HOUR|DAY|WEEK|MONTH|YEAR]
&#x9;DECLARE timerPeriodQty&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER; &#x9;-- Quantity of the period
&#x9;DECLARE timerRecurringDay &#x9;&#x9;&#x9;&#x9;VARCHAR;&#x9;-- NULL or a list of one or more items: [SUN, MON, TUE, WED, THU, FRI, SAT].  May be space or comma delimited.
&#x9;DECLARE timerFromTimeInADay&#x9;&#x9;&#x9;&#x9;TIME;&#x9;&#x9;-- NULL or The time of day to start the trigger.  Format = HH24:MM:SS
&#x9;DECLARE timerEndTimeInADay&#x9;&#x9;&#x9;&#x9;TIME;&#x9;&#x9;-- NULL or The time of day to end the trigger.   Format = HH24:MM:SS
&#x9;DECLARE timerIsCluster&#x9;&#x9;&#x9;&#x9;&#x9;BIT;&#x9;&#x9;-- NULL or 0=not once per cluster, 1=only once per cluster
&#x9;-- When conditionType=JMS set these variables
&#x9;DECLARE jmsDestination&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The JMS Destination
&#x9;DECLARE jmsSelector&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The JMS Selector
&#x9;DECLARE jmsConnection&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The JMS Connection string
&#x9;-- When conditionType=SYSTEM_EVENT set these variables
&#x9;DECLARE systemEventName&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- One of [CacheRefreshFailure|CacheRefreshSuccess|ClusterServerJoined|ClusterServerConnected|ClusterServerDisconnected|
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--        ClusterServerShunned|DataSourceDown|DataSourceUp|RequestFailure|RequestInactive|RequestRunForTooLong|ResourceLock
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--        ResourceUnlock|RequestsSpike|ErrorsSpike|FailedLoginSpike|StatisticsGatheringFailure|ServerStart|ServerStop|TransactionFailure]
&#x9;-- When conditionType=USER_DEFINED set these variables
&#x9;DECLARE userDefinedName&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- A user defined event

-- ACTION TYPE ATTRIBUTES:
&#x9;-- When actionType=PROCEDURE set these variables.  Execute a procedure.
&#x9;DECLARE procedurePath&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The procdure path to execute
&#x9;DECLARE procedureParam&#x9; &#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- Procedure parameters in a comma separated list.  Single quotes around string values.
&#x9;DECLARE procedureExhaustCursors&#x9;&#x9;&#x9;BIT;&#x9;&#x9;--
&#x9;-- When actionType=STATISTICS set these variables.  Gather statistics on a data source.
&#x9;DECLARE statisticsDSPath&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The path to the datasource for gathering statistics
&#x9;-- When actionType=REINTROSPECT set these variables.  Reintrospect a data source and send an email.
&#x9;DECLARE reintrospectDSPath&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The path to the datasource to reintrospect
&#x9;DECLARE reintrospectEmailTo&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The TO to send email for reintrospection
&#x9;DECLARE reintrospectEmailCC&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The CC to send email for reintrospection
&#x9;DECLARE reintrospectEmailBCC&#x9;&#x9;&#x9;LONGVARCHAR;-- The BCC to send email for reintrospection
&#x9;DECLARE reintrospectEmailReplyTo&#x9;&#x9;LONGVARCHAR;-- The REPLYTO to send email for reintrospection
&#x9;DECLARE reintrospectEmailSubject&#x9;&#x9;LONGVARCHAR;-- The subject line to send email for reintrospection
&#x9;DECLARE reintrospectEmailContent&#x9;&#x9;LONGVARCHAR;-- The content to send email for reintrospection
&#x9;DECLARE reintrospectEmailSkipNoResults &#x9;BIT;&#x9;&#x9;-- The skip if no results flag to send email for reintrospection
&#x9;DECLARE reintrospectEmailNoCommit&#x9;&#x9;BIT;&#x9;&#x9;-- The no commit flag to send email for reintrospection
&#x9;-- When actionType=EMAIL set these variables.  Execute a procedure or view and send results via email.
&#x9;DECLARE execEmailResourcePath&#x9;&#x9;&#x9;LONGVARCHAR;-- Path of resources (Procedure or View) to execute
&#x9;DECLARE execEmailResourceParam&#x9; &#x9;&#x9;LONGVARCHAR;-- Procedure parameters in a comma separated list.  Single quotes around string values.
&#x9;DECLARE execEmailTo&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The TO to send email for reintrospect
&#x9;DECLARE execEmailCC&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The CC to send email for reintrospect
&#x9;DECLARE execEmailBCC&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The BCC to send email for reintrospect
&#x9;DECLARE execEmailReplyTo&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The REPLYTO to send email for reintrospect
&#x9;DECLARE execEmailSubject&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The subject line to send email for reintrospect
&#x9;DECLARE execEmailContent&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- The Content to send email for reintrospect
&#x9;DECLARE execEmailSkipNoResults &#x9;&#x9;&#x9;BIT;&#x9;&#x9;-- The skip if no results flag to send email for reintrospect
&#x9;DECLARE execEmailIncludeSummary&#x9;&#x9;&#x9;BIT;&#x9;&#x9;-- The include summary flag to send email for reintrospect

-- GENERAL ATTRIBUTES:
    DECLARE maxEventsQueued &#x9;&#x9;&#x9;&#x9;INTEGER;&#x9;-- (optional): The maximum number of triggering events to queue.
    DECLARE annotation &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;-- (optional): A description of the resource.  If not provided, the annotation will be left unaltered.

&#x9;-- Procedure Parameters
&#x9;DECLARE i&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE attrName&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE attrValue&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE createInput &#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE conditionSchedule&#x9;&#x9;&#x9;&#x9;LONGVARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE conditionAttributes&#x9;&#x9;&#x9;&#x9;LONGVARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE actionTypeAttributes&#x9;&#x9;&#x9;LONGVARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE resourceAttributes&#x9;&#x9;&#x9;&#x9;LONGVARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE remainingInput&#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR DEFAULT &apos;&apos;;
&#x9;DECLARE timerMode&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR; &#x9;-- [INTERVAL|CALENDAR] - Derived ffrom timerPeriod.  INTERVAL=MINUTE, CALENDAR=HOUR|DAY|WEEK|MONTH|YEAR
&#x9;DECLARE fromTimeInADay&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT -1;
&#x9;DECLARE endTimeInADay&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT -1;
&#x9;DECLARE recurringDay&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT 0;
&#x9;DECLARE dayOfWeek&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE emailResourceType&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE resourceExists&#x9;&#x9;&#x9;&#x9;&#x9;BOOLEAN;
&#x9;DECLARE boolResponse1&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE boolResponse2&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;-- Assign attributes to variables
&#x9;SET i = 1;
&#x9;WHILE (i &lt;= CARDINALITY(triggerAttrVect)) DO
&#x9;&#x9;SET attrName = TRIM(triggerAttrVect[i].trName);
&#x9;&#x9;SET attrValue = TRIM(triggerAttrVect[i].trValue);

&#x9;&#x9;IF (attrName IS NOT NULL AND LENGTH(attrName) &gt; 0) THEN

&#x9;&#x9;&#x9;CASE attrName
&#x9;&#x9;&#x9;-- CONDITION TYPE ATTRIBUTES:
&#x9;&#x9;&#x9;&#x9;-- When conditionType=TIMER set these variables
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerStartTime&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET timerStartTime = CAST(attrValue AS TIMESTAMP);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerPeriod&apos; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN SET timerPeriod = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerPeriodQty&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET timerPeriodQty = CAST(attrValue AS INTEGER);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerRecurringDay&apos; &#x9;&#x9;&#x9;&#x9;THEN SET timerRecurringDay = CAST(attrValue AS VARCHAR);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerFromTimeInADay&apos; &#x9;&#x9;&#x9;&#x9;THEN SET timerFromTimeInADay = CAST(attrValue AS TIME);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerEndTimeInADay&apos; &#x9;&#x9;&#x9;&#x9;THEN SET timerEndTimeInADay = CAST(attrValue AS TIME);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;timerIsCluster&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET timerIsCluster = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;&#x9;-- When conditionType=JMS set these variables
&#x9;&#x9;&#x9;&#x9;WHEN &apos;jmsDestination&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET jmsDestination = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;jmsSelector&apos; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN SET jmsSelector = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;jmsConnection&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET jmsConnection = attrValue;
&#x9;&#x9;&#x9;&#x9;-- When conditionType=SYSTEM_EVENT set these variables
&#x9;&#x9;&#x9;&#x9;WHEN &apos;systemEventName&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET systemEventName = attrValue;
&#x9;&#x9;&#x9;&#x9;-- When conditionType=USER_DEFINED set these variables
&#x9;&#x9;&#x9;&#x9;WHEN &apos;userDefinedName&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET userDefinedName = attrValue;

&#x9;&#x9;&#x9;-- ACTION TYPE ATTRIBUTES:
&#x9;&#x9;&#x9;&#x9;-- When actionType=PROCEDURE set these variables.  Execute a procedure.
&#x9;&#x9;&#x9;&#x9;WHEN &apos;procedurePath&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET procedurePath = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;procedureParams&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET procedureParam = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;procedureExhaustCursors&apos;&#x9;&#x9;&#x9;THEN SET procedureExhaustCursors = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;&#x9;-- When actionType=STATISTICS set these variables.  Gather statistics on a data source.
&#x9;&#x9;&#x9;&#x9;WHEN &apos;statisticsDSPath&apos; &#x9;&#x9;&#x9;&#x9;THEN SET statisticsDSPath = attrValue;
&#x9;&#x9;&#x9;&#x9;-- When actionType=REINTROSPECT set these variables.  Reintrospect a data source and send an email.
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectDSPath&apos; &#x9;&#x9;&#x9;&#x9;THEN SET reintrospectDSPath = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailTo&apos; &#x9;&#x9;&#x9;&#x9;THEN SET reintrospectEmailTo = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailCC&apos; &#x9;&#x9;&#x9;&#x9;THEN SET reintrospectEmailCC = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailBCC&apos; &#x9;&#x9;&#x9;THEN SET reintrospectEmailBCC = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailReplyTo&apos; &#x9;&#x9;THEN SET reintrospectEmailReplyTo = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailSubject&apos; &#x9;&#x9;THEN SET reintrospectEmailSubject = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailContent&apos; &#x9;&#x9;THEN SET reintrospectEmailContent = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailSkipNoResults&apos;&#x9;THEN SET reintrospectEmailSkipNoResults = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;reintrospectEmailNoCommit&apos; &#x9;&#x9;THEN SET reintrospectEmailNoCommit = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;&#x9;-- When actionType=EMAIL set these variables.  Execute a procedure or view and send results via email.
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailResourcePath&apos; &#x9;&#x9;&#x9;THEN SET execEmailResourcePath = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailResourceParam&apos; &#x9;&#x9;&#x9;THEN SET execEmailResourceParam = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailTo&apos; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN SET execEmailTo = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailCC&apos; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN SET execEmailCC = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailBCC&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET execEmailBCC = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailReplyTo&apos; &#x9;&#x9;&#x9;&#x9;THEN SET execEmailReplyTo = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailSubject&apos; &#x9;&#x9;&#x9;&#x9;THEN SET execEmailSubject = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailContent&apos; &#x9;&#x9;&#x9;&#x9;THEN SET execEmailContent = attrValue;
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailSkipNoResults&apos;&#x9;&#x9;&#x9;THEN SET execEmailSkipNoResults = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;execEmailIncludeSummary&apos; &#x9;&#x9;&#x9;THEN SET execEmailIncludeSummary = CAST(attrValue AS BIT);
&#x9;&#x9;&#x9;-- GENERAL ATTRIBUTES:
&#x9;&#x9;&#x9;&#x9;WHEN &apos;maxEventsQueued&apos; &#x9;&#x9;&#x9;&#x9;&#x9;THEN SET maxEventsQueued = CAST(attrValue AS INTEGER);
&#x9;&#x9;&#x9;&#x9;WHEN &apos;annotation&apos; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN SET annotation = attrValue;
&#x9;&#x9;&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;END CASE;
&#x9;&#x9;END IF; 
&#x9;&#x9;set i = i + 1;
&#x9;END WHILE;

&#x9;-- Validate enabled
&#x9;IF (enabled IS NULL) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The enabled parameter may not be null or empty.&apos;;&#x9;
&#x9;END IF;

&#x9;-- Validate conditionType and associated values
&#x9;IF (conditionType IS NOT NULL AND (LENGTH(conditionType) = 0 OR UPPER(TRIM(conditionType)) NOT IN (&apos;TIMER&apos;,&apos;JMS&apos;,&apos;SYSTEM_EVENT&apos;,&apos;USER_DEFINED&apos;))) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The conditionType=[&apos;||NVL(conditionType,&apos;null&apos;)||&apos;] is not valid.&apos;;
&#x9;END IF;
&#x9;IF (conditionType IS NOT NULL AND TRIM(conditionType) = &apos;TIMER&apos;) THEN
&#x9;&#x9;IF (timerStartTime IS NOT NULL AND (timerPeriod IS NULL OR LENGTH(timerPeriod) = 0)) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The timerPeriod=[&apos;||NVL(timerPeriod,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (timerPeriod IS NOT NULL AND UPPER(TRIM(timerPeriod)) NOT IN (&apos;MINUTE&apos;,&apos;HOUR&apos;,&apos;DAY&apos;,&apos;WEEK&apos;,&apos;MONTH&apos;,&apos;YEAR&apos;)) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The timerPeriod=[&apos;||NVL(timerPeriod,&apos;null&apos;)||&apos;] is not valid.&apos;;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (timerPeriod IS NOT NULL AND timerPeriodQty IS NULL) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The timerPeriodQty=[&apos;||NVL(CAST(timerPeriodQty AS VARCHAR),&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;&#x9;FOR r AS 
&#x9;&#x9;&#x9;SELECT * FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(REPLACE(timerRecurringDay,&apos; &apos;,&apos;,&apos;), &apos;,&apos;,0,0,0,1,0)
&#x9;&#x9;&#x9;&#x9;WHERE object IS NOT NULL and LENGTH(object) &gt; 0 
&#x9;&#x9;DO
&#x9;&#x9;&#x9;SET dayOfWeek = r.object;
&#x9;&#x9;&#x9;IF (UPPER(TRIM(dayOfWeek)) NOT IN (&apos;SUN&apos;,&apos;MON&apos;,&apos;TUE&apos;,&apos;WED&apos;,&apos;THU&apos;,&apos;FRI&apos;,&apos;SAT&apos;,&apos;SUN&apos;)) THEN
&#x9;&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The timerRecurringDay[&apos;||NVL(timerRecurringDay,&apos;null&apos;)||&apos;] contains an invalid entry...dayOfWeek=[&apos;||dayOfWeek||&apos;].&apos;;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END FOR;
&#x9;END IF;
&#x9;IF (conditionType IS NOT NULL AND TRIM(conditionType) = &apos;JMS&apos;) THEN
&#x9;&#x9;IF (jmsDestination IS NULL OR LENGTH(jmsDestination) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The jmsDestination=[&apos;||NVL(jmsDestination,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (jmsSelector IS NULL OR LENGTH(jmsSelector) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The jmsSelector=[&apos;||NVL(jmsSelector,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (jmsConnection IS NULL OR LENGTH(jmsConnection) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The jmsConnection=[&apos;||NVL(jmsConnection,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;IF (conditionType IS NOT NULL AND TRIM(conditionType) = &apos;SYSTEM_EVENT&apos;) THEN
&#x9;&#x9;IF (systemEventName IS NULL OR LENGTH(systemEventName) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The systemEventName=[&apos;||NVL(systemEventName,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;IF (conditionType IS NOT NULL AND TRIM(conditionType) = &apos;USER_DEFINED&apos;) THEN
&#x9;&#x9;IF (userDefinedName IS NULL OR LENGTH(userDefinedName) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The userDefinedName=[&apos;||NVL(userDefinedName,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;END IF;

&#x9;-- Validate the actionType and associated values
&#x9;IF (conditionType IS NOT NULL AND (actionType IS NULL OR LENGTH(actionType) = 0)) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The actionType may not be null or empty.&apos;;
&#x9;END IF;
&#x9;IF (actionType IS NOT NULL AND (LENGTH(actionType) = 0 OR UPPER(TRIM(actionType)) NOT IN (&apos;PROCEDURE&apos;,&apos;STATISTICS&apos;,&apos;REINTROSPECT&apos;,&apos;EMAIL&apos;))) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The actionType=[&apos;||NVL(actionType,&apos;null&apos;)||&apos;] is not valid.&apos;;
&#x9;END IF;
&#x9;IF (actionType IS NOT NULL AND TRIM(actionType) = &apos;PROCEDURE&apos;) THEN
&#x9;&#x9;IF (procedurePath IS NULL OR LENGTH(procedurePath) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The procedurePath=[&apos;||NVL(procedurePath,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;CALL /lib/resource/ResourceExists(procedurePath, &apos;PROCEDURE&apos;, null, resourceExists);
&#x9;&#x9;&#x9;IF (NOT resourceExists) THEN
&#x9;&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The procedurePath=[&apos;||NVL(procedurePath,&apos;null&apos;)||&apos;] does not exist.&apos;;&#x9;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;IF (actionType IS NOT NULL AND TRIM(actionType) = &apos;STATISTICS&apos;) THEN
&#x9;&#x9;IF (statisticsDSPath IS NULL OR LENGTH(statisticsDSPath) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The statisticsDSPath=[&apos;||NVL(statisticsDSPath,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;CALL /lib/resource/ResourceExists(statisticsDSPath, &apos;DATA_SOURCE&apos;, null, resourceExists);
&#x9;&#x9;&#x9;IF (NOT resourceExists) THEN
&#x9;&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The statisticsDSPath=[&apos;||NVL(statisticsDSPath,&apos;null&apos;)||&apos;] does not exist.&apos;;&#x9;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;IF (actionType IS NOT NULL AND TRIM(actionType) = &apos;REINTROSPECT&apos;) THEN
&#x9;&#x9;IF (reintrospectDSPath IS NULL OR LENGTH(reintrospectDSPath) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The reintrospectDSPath=[&apos;||NVL(reintrospectDSPath,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;CALL /lib/resource/ResourceExists(reintrospectDSPath, &apos;DATA_SOURCE&apos;, null, resourceExists);
&#x9;&#x9;&#x9;IF (NOT resourceExists) THEN
&#x9;&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The reintrospectDSPath=[&apos;||NVL(reintrospectDSPath,&apos;null&apos;)||&apos;] does not exist.&apos;;&#x9;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (reintrospectEmailTo IS NULL OR LENGTH(reintrospectEmailTo) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The reintrospectEmailTo=[&apos;||NVL(reintrospectEmailTo,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;END IF;
&#x9;IF (actionType IS NOT NULL AND TRIM(actionType) = &apos;EMAIL&apos;) THEN
&#x9;&#x9;IF (execEmailResourcePath IS NULL OR LENGTH(execEmailResourcePath) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The execEmailResourcePath=[&apos;||NVL(execEmailResourcePath,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;SELECT &quot;type&quot; INTO emailResourceType FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/getBasicResourceCursor(execEmailResourcePath, NULL);
&#x9;&#x9;&#x9;CALL /lib/resource/ResourceExists(execEmailResourcePath, emailResourceType, null, resourceExists);
&#x9;&#x9;&#x9;IF (NOT resourceExists) THEN
&#x9;&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The execEmailResourcePath=[&apos;||NVL(execEmailResourcePath,&apos;null&apos;)||&apos;] does not exist.&apos;;&#x9;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END IF;
&#x9;&#x9;IF (execEmailTo IS NULL OR LENGTH(execEmailTo) = 0) THEN
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The execEmailTo=[&apos;||NVL(execEmailTo,&apos;null&apos;)||&apos;] parameter may not be null or empty.&apos;;&#x9;
&#x9;&#x9;END IF;
&#x9;END IF;

&#x9;SET success = 1;
 &#x9;CALL /lib/resource/ResourceExists(fullResourcePath, &apos;TRIGGER&apos;, null, resourceExists);
&#x9;IF (resourceExists) THEN
&#x9;&#x9;IF (conditionType IS NULL) THEN
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/conversions/convertBoolean(CAST(enabled AS VARCHAR), boolResponse1);
&#x9;&#x9;&#x9;set createInput = 
&#x9;&#x9;&#x9;&#x9;&apos;&lt;resource:updateTrigger &apos;||CHR(10)||
                &apos;    xmlns:resource=&quot;http://www.compositesw.com/services/system/admin/resource&quot; &apos;||CHR(10)||
                &apos;    xmlns:ns1=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:path&gt;&apos;||CAST(XMLTEXT(fullResourcePath) AS LONGVARCHAR)||&apos;&lt;/resource:path&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:enabled&gt;&apos;||boolResponse1||&apos;&lt;/resource:enabled&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:detail&gt;SIMPLE&lt;/resource:detail&gt;&apos;||CHR(10)||
                &apos;&lt;/resource:updateTrigger&gt;&apos;;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the TIMER condition values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(conditionType) = &apos;TIMER&apos;) THEN
&#x9;&#x9;&#x9;&#x9;-- Set the timer mode
&#x9;&#x9;&#x9;&#x9;SET timerMode = &apos;CALENDAR&apos;;
&#x9;&#x9;&#x9;&#x9;IF (UPPER(TRIM(timerPeriod)) = &apos;MINUTE&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;SET timerMode = &apos;INTERVAL&apos;; 
&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;IF (timerStartTime IS NULL) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;SET timerMode = &apos;NONE&apos;;
&#x9;&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;&#x9;-- Set the recurring day value
&#x9;&#x9;&#x9;&#x9;FOR r AS 
&#x9;&#x9;&#x9;&#x9;&#x9;SELECT * FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(REPLACE(timerRecurringDay,&apos; &apos;,&apos;,&apos;), &apos;,&apos;,0,0,0,1,0)
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE object IS NOT NULL and LENGTH(object) &gt; 0 
&#x9;&#x9;&#x9;&#x9;DO
&#x9;&#x9;&#x9;&#x9;&#x9;SET dayOfWeek = UPPER(r.object);
&#x9;&#x9;&#x9;&#x9;&#x9;CASE
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;SUN&apos; THEN set recurringDay = recurringDay + 1; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;MON&apos; THEN set recurringDay = recurringDay + 2; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;TUE&apos; THEN set recurringDay = recurringDay + 4; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;WED&apos; THEN set recurringDay = recurringDay + 8; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;THU&apos; THEN set recurringDay = recurringDay + 16; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;FRI&apos; THEN set recurringDay = recurringDay + 32; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN dayOfWeek = &apos;SAT&apos; THEN set recurringDay = recurringDay + 64; 
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;&#x9;&#x9;END CASE;
&#x9;&#x9;&#x9;&#x9;END FOR;
&#x9;&#x9;&#x9;&#x9;IF (recurringDay = 0) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;SET recurringDay = -1;
&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;-- Set and validate recurrence restriction timer
&#x9;&#x9;&#x9;&#x9;IF (timerFromTimeInADay IS NOT NULL AND timerEndTimeInADay IS NOT NULL) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;-- Set the from time in a day value
&#x9;&#x9;&#x9;&#x9;&#x9;IF (timerFromTimeInADay IS NOT NULL) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set fromTimeInADay = EXTRACT(HOUR FROM timerFromTimeInADay) * 60;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;IF (EXTRACT(MINUTE FROM timerFromTimeInADay) &gt;= 30) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set fromTimeInADay = fromTimeInADay + 30;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;&#x9;-- Set the end time in a day value
&#x9;&#x9;&#x9;&#x9;&#x9;IF (timerEndTimeInADay IS NOT NULL) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set endTimeInADay = EXTRACT(HOUR FROM timerEndTimeInADay) * 60;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;IF (EXTRACT(MINUTE FROM timerEndTimeInADay) &gt;= 30) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set endTimeInADay = endTimeInADay + 30;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = 
                &apos;    &lt;resource:conditionType&gt;TIMER&lt;/resource:conditionType&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:conditionSchedule&gt;&apos;||CHR(10)||
                &apos;        &lt;resource:mode&gt;&apos;||CAST(XMLTEXT(timerMode) AS VARCHAR)||&apos;&lt;/resource:mode&gt;&apos;||CHR(10);

&#x9;&#x9;&#x9;&#x9;IF (timerMode != &apos;NONE&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = conditionSchedule || 
                &apos;        &lt;resource:startTime&gt;&apos;||CAST(XMLTEXT(timerStartTime) AS VARCHAR)||&apos;&lt;/resource:startTime&gt;&apos;||CHR(10);

&#x9;&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = conditionSchedule||
                &apos;        &lt;resource:fromTimeInADay&gt;&apos;||CAST(XMLTEXT(fromTimeInADay) AS VARCHAR)||&apos;&lt;/resource:fromTimeInADay&gt;&apos;||CHR(10)||
                &apos;        &lt;resource:endTimeInADay&gt;&apos;||CAST(XMLTEXT(endTimeInADay) AS VARCHAR)||&apos;&lt;/resource:endTimeInADay&gt;&apos;||CHR(10)||
                &apos;        &lt;resource:recurringDay&gt;&apos;||CAST(XMLTEXT(recurringDay) AS VARCHAR)||&apos;&lt;/resource:recurringDay&gt;&apos;||CHR(10);

&#x9;&#x9;&#x9;&#x9;&#x9;IF (timerMode = &apos;INTERVAL&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = conditionSchedule||
                &apos;        &lt;resource:interval&gt;&apos;||CAST(XMLTEXT(timerPeriodQty * 60) AS VARCHAR)||&apos;&lt;/resource:interval&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;&#x9;&#x9;IF (timerMode = &apos;CALENDAR&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = conditionSchedule||
                &apos;        &lt;resource:period&gt;&apos;||CAST(XMLTEXT(UPPER(timerPeriod)) AS VARCHAR)||&apos;&lt;/resource:period&gt;&apos;||CHR(10)||
                &apos;        &lt;resource:count&gt;&apos;||CAST(XMLTEXT(timerPeriodQty) AS VARCHAR)||&apos;&lt;/resource:count&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/conversions/convertBoolean(NVL(CAST(timerIsCluster AS VARCHAR), &apos;0&apos;), boolResponse1);
&#x9;&#x9;&#x9;&#x9;SET conditionSchedule = conditionSchedule||
                &apos;        &lt;resource:isCluster&gt;&apos;||boolResponse1||&apos;&lt;/resource:isCluster&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:conditionSchedule&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the JMS condition values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(conditionType) = &apos;JMS&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET conditionAttributes = 
                &apos;    &lt;resource:conditionType&gt;JMS&lt;/resource:conditionType&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:conditionAttributes&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;JMS_DESTINATION&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(TRIM(NVL(jmsDestination,&apos;  &apos;))) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;       &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;JMS_SELECTOR&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(TRIM(NVL(jmsSelector,&apos;  &apos;))) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;       &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;JMS_CONNECTOR&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(TRIM(NVL(jmsConnection,&apos;  &apos;))) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;       &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:conditionAttributes&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the SYSTEM_EVENT condition values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(conditionType) = &apos;SYSTEM_EVENT&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET conditionAttributes = 
                &apos;    &lt;resource:conditionType&gt;SYSTEM_EVENT&lt;/resource:conditionType&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:conditionAttributes&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;EVENT_NAME&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(systemEventName) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;       &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:conditionAttributes&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the USER_DEFINED condition values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(conditionType) = &apos;USER_DEFINED&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET conditionAttributes = 
                &apos;    &lt;resource:conditionType&gt;USER_DEFINED&lt;/resource:conditionType&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:conditionAttributes&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;NAME&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(userDefinedName) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;       &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:conditionAttributes&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;

&#x9;&#x9;-- BEGIN actionType --
&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the PROCEDURE action attribute values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(actionType) = &apos;PROCEDURE&apos;) THEN
&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/conversions/convertBoolean(NVL(CAST(procedureExhaustCursors AS VARCHAR), &apos;0&apos;), boolResponse1);
&#x9;&#x9;&#x9;&#x9;SET actionTypeAttributes = 
                &apos;    &lt;resource:actionType&gt;PROCEDURE&lt;/resource:actionType&gt;&apos;||CHR(10)||
                &apos;      &lt;resource:actionAttributes&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;          &lt;common:name&gt;PATH&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;          &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;          &lt;common:value&gt;&apos;||CAST(XMLTEXT(TRIM(NVL(procedurePath,&apos;  &apos;))) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;        &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;          &lt;common:name&gt;PARAMETERS&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;          &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;          &lt;common:value&gt;&apos;|| CAST(XMLTEXT(TRIM(NVL(procedureParam,&apos;  &apos;))) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;        &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;          &lt;common:name&gt;EXHAUST_CURSORS&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;          &lt;common:type&gt;BOOLEAN&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;          &lt;common:value&gt;&apos;||boolResponse1||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;        &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:actionAttributes&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the STATISTICS action attribute values
&#x9;&#x9;&#x9; *******************************************/
&#x9;&#x9;&#x9;IF (TRIM(actionType) = &apos;STATISTICS&apos;) THEN
&#x9;&#x9;&#x9;&#x9;SET actionTypeAttributes = 
                &apos;    &lt;resource:actionType&gt;STATISTICS&lt;/resource:actionType&gt;&apos;||CHR(10)||
                &apos;    &lt;resource:actionAttributes&gt;&apos;||CHR(10)||
                &apos;        &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
                &apos;            &lt;common:name&gt;PATH&lt;/common:name&gt;&apos;||CHR(10)||
                &apos;            &lt;common:type&gt;STRING&lt;/common:type&gt;&apos;||CHR(10)||
                &apos;            &lt;common:value&gt;&apos;||CAST(XMLTEXT(statisticsDSPath) AS LONGVARCHAR)||&apos;&lt;/common:value&gt;&apos;||CHR(10)||
                &apos;        &lt;/common:attribute&gt;&apos;||CHR(10)||
                &apos;    &lt;/resource:actionAttributes&gt;&apos;||CHR(10);
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;/*******************************************
&#x9;&#x9;&#x9; * Set the REINTROSPECT action attribute values
