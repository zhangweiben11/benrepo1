PROCEDURE createFolder(
&#x9;IN sourceFolderPath &#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType,
&#x9;OUT success&#x9;  &#x9;BIT,
&#x9;OUT faultResponse &#x9;XML
)
BEGIN
    PATH /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log;
    PATH /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository;

    DECLARE PROC_NAME       CONSTANT VARCHAR DEFAULT &apos;repository/createFolder&apos;;
&#x9;DECLARE debug&#x9;&#x9;&#x9;CHAR(1) DEFAULT /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug;
&#x9;DECLARE folderPath  &#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType DEFAULT sourceFolderPath;
&#x9;DECLARE folderName&#x9;&#x9;VARCHAR;
&#x9;DECLARE resourceExists &#x9;BOOLEAN;
&#x9;DECLARE createInput &#x9;VARCHAR(8128);
&#x9;DECLARE createResponse &#x9;XML;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;EXCEPTION;

    set success = 1;
&#x9;if (folderPath IS NULL or LENGTH(folderPath) = 0) then
&#x9;&#x9;raise ex value &apos;Source input path may not be null or empty.&apos;;
