PROCEDURE getCurrentTimestamp(
&#x9;OUT currentTS&#x9;TIMESTAMP
)
BEGIN
&#x9;DECLARE resultResponse&#x9;&#x9;XML;
&#x9;DECLARE faultResponse&#x9;&#x9;XML;
&#x9;DECLARE pos1, pos2, len&#x9;&#x9;INTEGER;
&#x9;DECLARE resultStr&#x9;&#x9;&#x9;LONGVARCHAR;

&#x9;CALL /services/webservices/system/admin/server/operations/getServerAttributes(
&apos;&lt;server:getServerAttributes xmlns:server=&quot;http://www.compositesw.com/services/system/admin/server&quot;&gt;
  &lt;server:paths&gt;
    &lt;server:path&gt;/server/config/info/currentTime&lt;/server:path&gt;
  &lt;/server:paths&gt;
&lt;/server:getServerAttributes&gt;&apos;, resultResponse, faultResponse);

&#x9;-- Result &lt;common:value&gt;2020-05-24T07:48:31.938Z&lt;/common:value&gt;
&#x9;SET resultStr = CAST(resultResponse AS LONGVARCHAR);
&#x9;SET pos1 = INSTR(resultStr, &apos;&lt;common:value&gt;&apos;);
&#x9;SET pos2 = INSTR(resultStr, &apos;&lt;/common:value&gt;&apos;);
&#x9;SET len = LENGTH(&apos;&lt;common:value&gt;&apos;);
&#x9;SET resultStr = SUBSTRING(resultStr, pos1+len, pos2-pos1-len);
&#x9;--CALL print(&apos;resultStr=&apos;||resultStr);
&#x9;SET currentTS = CAST(resultStr AS TIMESTAMP);
END</attribute>
