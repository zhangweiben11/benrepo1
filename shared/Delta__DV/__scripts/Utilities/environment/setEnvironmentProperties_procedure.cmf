  <attribute name="Script" type="STRING">/*
&#x9;setEnvironmentProperties:

&#x9;This procedure is used to set TDV environment properties from either a TDV procedure that is properly formatted or 
&#x9;&#x9;from a properties file on the TDV server that is properly formatted.

&#x9;INPUT:
&#x9;&#x9;debug&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug on.  N=debug off.
&#x9;&#x9;resourcePath&#x9;&#x9;&#x9;VARCHAR(4000), &#x9;-- TDV resource procedure path or file system path.  It checks TDV path first

&#x9;&#x9;--   File System Path=/opt/TIBCO/TDV.properties   or    C:\TEMP\TDV.properties
&#x9;&#x9;&#x9;&#x9;Example of TDV.properties file:
&#x9;&#x9;&#x9;&#x9;&#x9;# test
&#x9;&#x9;&#x9;&#x9;&#x9;prop1=1
&#x9;&#x9;&#x9;&#x9;&#x9;prop2=2
&#x9;&#x9;&#x9;&#x9;&#x9;# end test
&#x9;&#x9;--   TDV Path=/shared/myproject/TDV_properties
&#x9;&#x9;&#x9;&#x9;Example of TDV_properties file:
&#x9;&#x9;&#x9;&#x9;&#x9;PROCEDURE TDV_properties(
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;out prop1 varchar,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;out prop2 varchar
&#x9;&#x9;&#x9;&#x9;&#x9;)
&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set prop1=&apos;11111&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set prop2=&apos;22222&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;END

&#x9;OUTPUT:
&#x9;&#x9;numPropertiesSet&#x9;&#x9;INTEGER&#x9;&#x9;-- The number of properties that were set.

&#x9;EXCEPTION:
&#x9;&#x9;ex&#x9;- An exception is thrown if the procedure cannot find the resource path or there was no resource path.

&#x9;Release:&#x9;Modified Date:&#x9;Modified By:&#x9;&#x9;DV Version:&#x9;&#x9;Reason:
&#x9;2020.300&#x9;08/30/2020&#x9;&#x9;Mike Tinius&#x9;&#x9;&#x9;7.0.8&#x9;&#x9;&#x9;Created new

&#x9;(c) 2017 TIBCO Software Inc.  All rights reserved.
&#x9;
&#x9;Except as specified below, this software is licensed pursuant to the Eclipse Public License v. 1.0.
&#x9;The details can be found in the file LICENSE.
&#x9;
&#x9;The following proprietary files are included as a convenience, and may not be used except pursuant
&#x9;to valid license to Composite Information Server or TIBCOÂ® Data Virtualization Server:
&#x9;csadmin-XXXX.jar, csarchive-XXXX.jar, csbase-XXXX.jar, csclient-XXXX.jar, cscommon-XXXX.jar,
&#x9;csext-XXXX.jar, csjdbc-XXXX.jar, csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar,
&#x9;and customproc-XXXX.jar (where -XXXX is an optional version number).  Any included third party files
&#x9;are licensed under the terms contained in their own accompanying LICENSE files, generally named .LICENSE.txt.
&#x9;
&#x9;This software is licensed AS-IS. Support for this software is not covered by standard maintenance agreements with TIBCO.
&#x9;If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
&#x9;agreement with TIBCO.
*/
PROCEDURE setEnvironmentProperties(
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug on.  N=debug off.
&#x9;IN resourcePath&#x9;&#x9;&#x9;VARCHAR(4000), &#x9;-- TDV resource procedure path or file system path.  It checks TDV path first
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--   File System Path=/opt/TIBCO/TDV.properties   or    C:\TEMP\TDV.properties
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--   TDV Path=/shared/myproject/TDV_properties
&#x9;OUT numPropertiesSet&#x9;INTEGER&#x9;&#x9;&#x9;-- The number of properties that were set.
)
BEGIN
&#x9;DECLARE resExists&#x9;&#x9;BOOLEAN;
&#x9;DECLARE content&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE line&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE pname&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE pvalue&#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE pos&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE setpos&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;IF (resourcePath IS NULL OR LENGTH(TRIM(resourcePath)) = 0) THEN
&#x9;&#x9;RAISE ex VALUE &apos;The input parameter &quot;resourcePath&quot; cannot be null or empty.&apos;;
&#x9;END IF;
&#x9;SET numPropertiesSet = 0;

&#x9;CALl /lib/resource/ResourceExists(resourcePath, &apos;PROCEDURE&apos;, null, resExists);
&#x9;IF (resExists) THEN
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/getScriptText(resourcePath, &apos;PROCEDURE&apos;, null, content);
&#x9;&#x9;FOR r AS 
&#x9;&#x9;&#x9;SELECT object
&#x9;&#x9;&#x9;FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(content, CHR(10), 0,0,0,0,0)
&#x9;&#x9;&#x9;WHERE object IS NOT NULL AND LENGTH(TRIM(object)) &gt; 0 AND SUBSTRING(object, 1, 1) != &apos;#&apos;
&#x9;&#x9;DO
&#x9;&#x9;&#x9;SET line = TRIM(r.object);
&#x9;&#x9;&#x9;SET setpos = INSTR(LOWER(line), &apos;set&apos;);
&#x9;&#x9;&#x9;SET pos = INSTR(line, &apos;=&apos;);
&#x9;&#x9;&#x9;IF (setpos &gt; 0 AND pos &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;SET pname = TRIM(REPLACE(REPLACE(SUBSTRING(line, 1, pos-1),&apos;SET&apos;,&apos;&apos;),&apos;set&apos;,&apos;&apos;));
&#x9;&#x9;&#x9;&#x9;IF (pname IS NOT NULL AND LENGTH(TRIM(pname)) &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/getConstantV2(resourcePath, pname, 1, pvalue);
&#x9;&#x9;&#x9;&#x9;&#x9;SET numPropertiesSet = numPropertiesSet + 1;
&#x9;&#x9;&#x9;&#x9;&#x9;CALL /lib/util/SetEnvironment(pname, pvalue);
&#x9;&#x9;&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CALL /lib/util/GetEnvironment(pname, pvalue);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;DEBUG: pname=[&apos;||pname||&apos;]  pvalue=[&apos;||NVL(pvalue,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;END FOR;
&#x9;ELSE -- File system path
&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/existsFile(resourcePath, resExists);
&#x9;&#x9;IF (resExists) THEN
&#x9;&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/getFileContentsAscii(resourcePath, content);
&#x9;&#x9;&#x9;FOR r AS 
&#x9;&#x9;&#x9;&#x9;SELECT object
&#x9;&#x9;&#x9;&#x9;FROM /shared/Delta_DV/&quot;_scripts&quot;/Utilities/string/extractDelimitedText(content, CHR(10), 0,0,0,0,0)
&#x9;&#x9;&#x9;&#x9;WHERE object IS NOT NULL AND LENGTH(TRIM(object)) &gt; 0 AND SUBSTRING(object, 1, 1) != &apos;#&apos;
&#x9;&#x9;&#x9;DO
&#x9;&#x9;&#x9;&#x9;SET line = r.object;
&#x9;&#x9;&#x9;&#x9;SET pos = INSTR(line, &apos;=&apos;);
&#x9;&#x9;&#x9;&#x9;IF (pos &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;SET pname = SUBSTRING(line, 1, pos-1);
&#x9;&#x9;&#x9;&#x9;&#x9;SET pvalue = SUBSTRING(line, pos+1);
&#x9;&#x9;&#x9;&#x9;&#x9;IF (pname IS NOT NULL AND LENGTH(TRIM(pname)) &gt; 0) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SET numPropertiesSet = numPropertiesSet + 1;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CALL /lib/util/SetEnvironment(pname, pvalue);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;IF (UPPER(debug) = &apos;Y&apos;) THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CALL /lib/util/GetEnvironment(pname, pvalue);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CALL PRINT(&apos;DEBUG: pname=[&apos;||pname||&apos;]  pvalue=[&apos;||NVL(pvalue,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;&#x9;END IF;
&#x9;&#x9;&#x9;END FOR;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;RAISE ex VALUE &apos;The &quot;resourcePath&quot; could not be found at path=[&apos;||resourcePath||&apos;]&apos;;
&#x9;&#x9;END IF;
&#x9;END IF;
END</attribute>
