PROCEDURE isEmpty(
&#x9;IN request LONGVARCHAR,
&#x9;OUT response BIT
)
BEGIN
&#x9;SET response = 0;
&#x9;IF (request IS NULL OR LENGTH(&apos;|&apos;||request||&apos;|&apos;) = 2) THEN
&#x9;&#x9;SET response = 1;
&#x9;END IF;
END</attribute>
