PROCEDURE exportResources(
&#x9;IN  debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug on.  N=debug off.
&#x9;IN  optionType&#x9;&#x9;&#x9;INTEGER,
&#x9;&#x9;&#x9;-- 0: Export a single resource, folder, or double-piped resource list with or without children as per the input variable &quot;includeChildren&quot;
&#x9;&#x9;&#x9;-- 1: Find the n most recent modified resources in the path (default of n is 1).  results are not ordered
&#x9;&#x9;&#x9;-- 2: Find the n least recent modified resources in the path (default of n is 1). results are not ordered
&#x9;&#x9;&#x9;-- 3: Find all resources modified after the passed in resource date
&#x9;&#x9;&#x9;-- 4: Find all resources modifed before the passed in resource date
&#x9;&#x9;&#x9;-- 5: Find all resources created after the passed in resource date
&#x9;&#x9;&#x9;-- 6: Find all resources created before the passed in resource date
&#x9;&#x9;&#x9;-- 7: [Lineage + modification date] Find the lineage of the passed in resource path where the modification date is after the passed in resource date and exclude paths if applicable.
&#x9;IN  resourcePathList&#x9;LONGVARCHAR,&#x9;-- May be an explicit single resource, a container path, a data source path, a published path or a double pipe-separated list of resources to export.
&#x9;IN  resourceNum&#x9;&#x9;&#x9;INTEGER,&#x9;&#x9;-- The number of resources to return in the list for optionType 1 or 2.  The default is 1
&#x9;IN  resourceDate&#x9;&#x9;TIMESTAMP,&#x9;&#x9;-- A timestamp to compare with the various resources for optionType 3-7.
&#x9;IN&#x9;excludePaths &#x9;&#x9;LONGVARCHAR,&#x9;-- A comma-separated list of paths to exclude from the output.  The INSTR function is used so that the paths can be partial paths.  Used by option 7.
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;Values: e.g. /shared/lab0/Physical/Metadata/   -- Exclude all physical metadata resources
&#x9;IN includeChildren&#x9;&#x9;VARCHAR, &#x9;&#x9;-- true(default) or false.  If the resource path is a CONTAINER or DATA_SOURCE then includeChildren=true will bring all children of that resource.

    IN archiveFilePath &#x9;&#x9;VARCHAR(4096),&#x9;-- the full path to the export archive (car) file on the TDV server file system.
&#x9;IN exportOptions&#x9;&#x9;LONGVARCHAR,&#x9;-- a pipe separated list of export options
    OUT success &#x9;&#x9;&#x9;BIT
)
BEGIN
&#x9;DECLARE resourceListXML&#x9;LONGVARCHAR;

&#x9;-- Derive the resource list
&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/archive/getExportResourceList(debug, optionType, resourcePathList, resourceNum, resourceDate, excludePaths, includeChildren, resourceListXML);

&#x9;-- Export the resources to an archive (car) file on the TDV file system.
&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/archive/exportArchiveFile(debug, archiveFilePath, resourceListXML, exportOptions, success);
END</attribute>
