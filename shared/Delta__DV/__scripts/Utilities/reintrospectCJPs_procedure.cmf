&#x9;csadmin-XXXX.jar, csarchive-XXXX.jar, csbase-XXXX.jar, csclient-XXXX.jar, cscommon-XXXX.jar,
&#x9;csext-XXXX.jar, csjdbc-XXXX.jar, csserverutil-XXXX.jar, csserver-XXXX.jar, cswebapi-XXXX.jar,
&#x9;and customproc-XXXX.jar (where -XXXX is an optional version number).  Any included third party files
&#x9;are licensed under the terms contained in their own accompanying LICENSE files, generally named .LICENSE.txt.
&#x9;
&#x9;This software is licensed AS-IS. Support for this software is not covered by standard maintenance agreements with TIBCO.
&#x9;If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
&#x9;agreement with TIBCO.

*/
PROCEDURE reintrospectCJPs(
    OUT success BIT
)
BEGIN
&#x9;DECLARE result&#x9;&#x9;BOOLEAN;
&#x9;DECLARe error&#x9;&#x9;LONGVARCHAR;

    -- update if the Utilties are not installed in /shared/ASAssets/Utilities
    --
    DECLARE UTILITIES_HOME CONSTANT /lib/resource/ResourceDefs.ResourcePath DEFAULT &apos;/shared/ASAssets/Utilities&apos;;

    DECLARE reintrospectID &#x9;&#x9;/lib/resource/ResourceDefs.ReintrospectId;
    DECLARE operationStatus &#x9;System.OperationStatus;
    DECLARE reintrospectReport &#x9;/lib/resource/ResourceDefs.ReintrospectReport;

    SET success = 1;

    -- iterate through the list of CJP data sources found in UTILITIES_HOME
    --
    FOR r AS 
        SELECT 
            resPath 
        FROM 
            /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/getResourceListRecursive (UTILITIES_HOME, &apos;CONTAINER&apos;) 
        WHERE 
            resType = &apos;DATA_SOURCE&apos; AND 
            subType = &apos;NONE&apos; AND
&#x9;&#x9;&#x9;name &lt;&gt; &apos;localRestServices&apos;
    DO
        CALL PRINT (&apos;Reintrospecting CJP data source &apos; || r.resPath || &apos; ...&apos;);

&#x9;&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;&#x9;-- initiate the reintrospection
&#x9;&#x9;&#x9;CALL /lib/resource/ReintrospectDataSource (r.resPath, reintrospectID);

&#x9;&#x9;&#x9;-- wait for the reintrospection to complete
&#x9;&#x9;&#x9;-- block until reintrospect complete 
&#x9;&#x9;&#x9;CALL /lib/resource/GetDataSourceReintrospectReport(reintrospectID, TRUE, operationStatus, reintrospectReport);

&#x9;&#x9;&#x9;CALL PRINT (&apos;Done. (OperationStatus = &apos;&apos;&apos; || operationStatus || &apos;&apos;&apos;)&apos;);
&#x9;&#x9;EXCEPTION
&#x9;&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;&#x9;SET error = CAST(CURRENT_EXCEPTION.TRACE AS LONGVARCHAR);
&#x9;&#x9;&#x9;&#x9;CALL PRINT (&apos;EXCEPTION: &apos; || error);
 &#x9;&#x9;&#x9;&#x9;SET success = 0;
&#x9;&#x9;END;


&#x9;&#x9;IF (INSTR(r.resPath, &apos;RepoUtils&apos;) &gt; 0) THEN
&#x9;&#x9;&#x9;-- Force writing out a new RepoUtils.properties file
&#x9;&#x9;&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/RepoUtils/ForceWriteRepoUtils(result);
&#x9;&#x9;&#x9;CALL PRINT (&apos;Done. (ForceWriteRepoUtils = &apos;&apos;&apos; || cast(result as varchar) || &apos;&apos;&apos;)&apos;);
&#x9;&#x9;END IF; 
    END FOR;

END</attribute>
