PROCEDURE copyAll(
&#x9;IN sourcePath&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType,
&#x9;IN targetPath&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType,
&#x9;INOUT mkdirCount&#x9;INTEGER,
&#x9;INOUT copyCount&#x9;&#x9;INTEGER
)
BEGIN
&#x9;PATH /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP;
    PATH /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log;
    PATH /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository;

    DECLARE PROC_NAME       CONSTANT VARCHAR DEFAULT &apos;file/copyAll&apos;;

&#x9;DECLARE folderName&#x9;&#x9;VARCHAR;
&#x9;DECLARE folderPath&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE newTargetPath&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;
&#x9;DECLARE success&#x9;&#x9;&#x9;BOOLEAN;
&#x9;DECLARE rootName&#x9;&#x9;VARCHAR;
&#x9;DECLARE rootPath&#x9;&#x9;/shared/Delta_DV/&quot;_scripts&quot;/Utilities/TypeDefinitions.pathType;

&#x9;if (mkdirCount is null) then
&#x9;&#x9;set mkdirCount = 0;
&#x9;end if;
&#x9;if (copyCount is null) then
&#x9;&#x9;set copyCount = 0;
&#x9;end if;
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/returnFolderNameAndFolderPath(sourcePath, &apos;E&apos;, rootName, folderPath);
&#x9;set rootPath = targetPath||&apos;/&apos;||rootName;

&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;SOURCE: &apos;||sourcePath);
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;TARGET: &apos;||targetPath);
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;ROOT:   &apos;||rootName);
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;   MKDIR: &apos;||rootPath);
&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/makeDirs(rootPath, success);

&#x9;FOR r as select * from /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/getFileInfo(sourcePath,&apos;Y&apos;) DO
&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/returnFolderNameAndFolderPath(r.filePath, &apos;E&apos;, folderName, folderPath);
&#x9;&#x9;set newTargetPath = rootPath||&apos;/&apos;|| folderName;

&#x9;&#x9;if (r.isDir = 1) then
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;   MKDIR: &apos;||newTargetPath);
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/makeDirs(newTargetPath, success);
&#x9;&#x9;&#x9;set mkdirCount = mkdirCount + 1;
&#x9;&#x9;&#x9;-- Recursively copy and create directories driven by the source path.
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/copyAll(r.filePath, rootPath, mkdirCount, copyCount);
&#x9;&#x9;end if;
&#x9;&#x9;if (r.isFile = 1) then
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;   COPY-SOURCE: &apos;||r.filePath);
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(PROC_NAME, /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/&quot;_debug&quot;.debug, &apos;        TARGET: &apos;||newTargetPath);
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/file/FileProcessingCJP/copyFile(r.filePath,newTargetPath);
&#x9;&#x9;&#x9;set copyCount = copyCount + 1;
&#x9;&#x9;end if;
&#x9;END FOR;
END</attribute>
