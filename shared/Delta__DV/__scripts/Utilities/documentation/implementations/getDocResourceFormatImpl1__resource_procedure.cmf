&#x9;&#x9;&#x9;&#x9;&#x9;set formattedText = formattedText || &apos;None&apos; || eol;
&#x9;&#x9;&#x9;&#x9;&#x9;set formattedText = formattedText || eol;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/*****************************************************************************
