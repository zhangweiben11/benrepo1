&#x9;set formattedText = &apos;&apos;;
&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debugTime,&apos;Overall Resource Start:: &apos;||RPAD(NVL(resourceType,&apos;null&apos;),30,&apos; &apos;)||NVL(resourcePath,&apos;null&apos;));
&#x9;end if;

&#x9;-- Retrieve information about the resource such as key elements: impactLevel and scriptText
&#x9;for r as select * from /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/getBasicResourceCursor(resourcePath, resourceType) do
&#x9;/*&#x9;CURSOR (
&#x9;&#x9;name VARCHAR(255), &#x9;&#x9;&#x9;-- name of the resource
&#x9;&#x9;&quot;path&quot; VARCHAR(32768), &#x9;&#x9;-- full path to the resource
&#x9;&#x9;&quot;type&quot; VARCHAR(32768), &#x9;&#x9;-- type of resource as described in the TYPE/SUBTYPE section below
