&#x9;&#x9;&#x9;end if;
&#x9;&#x9;end if;
&#x9;end for;

&#x9;set duration = CURRENT_TIMESTAMP - startTimeOverall;
&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debugTime,&apos;      Overall Resource::  duration=&apos;||cast(duration as varchar) || &apos;  resourcePath=&apos;||NVL(resourcePath,&apos;null&apos;));
