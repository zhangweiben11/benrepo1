&#x9;&#x9;&#x9;if (commandOptionValue = 1) then
&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/modules/getDocResourceProjection(r.&quot;path&quot;, r.&quot;type&quot;, constantPath, moduleText);
&#x9;&#x9;&#x9;&#x9;if (moduleText is null) then
&#x9;&#x9;&#x9;&#x9;&#x9;set moduleText = &apos;&apos;;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug,&apos;------getDocResourceProjection::&apos;||eol||moduleText);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (moduleText IS NOT NULL AND LENGTH(&apos;|&apos;||moduleText||&apos;|&apos;) &gt; 2) then
&#x9;&#x9;&#x9;&#x9;&#x9;set formattedText = formattedText || moduleText;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;set duration = CURRENT_TIMESTAMP - startTimeColumnProjections;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debugTime,&apos;    Column Projections::  duration=&apos;||cast(duration as varchar));
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/*****************************************************************************
&#x9;&#x9;&#x9; * Format resources used by this resource
&#x9;&#x9;&#x9; *****************************************************************************/
&#x9;&#x9;&#x9;-- Parse the switches for the &apos;print_resources_used&apos; option
&#x9;&#x9;&#x9;set startTimeResourcesUsed = CURRENT_TIMESTAMP;
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/helpers/parseDocSwitches(switches, &apos;print_resources_used&apos;, commandOptionValue);
&#x9;&#x9;&#x9;if (commandOptionValue = 1) then
&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/modules/getDocResourcesUsed(r.&quot;path&quot;, r.&quot;type&quot;, constantPath, moduleText);
&#x9;&#x9;&#x9;&#x9;if (moduleText is null) then
&#x9;&#x9;&#x9;&#x9;&#x9;set moduleText = &apos;&apos;;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/log/logDebugMessage(moduleName,debug,&apos;------getDocResourcesUsed::&apos;||eol||moduleText);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (moduleText IS NOT NULL AND LENGTH(&apos;|&apos;||moduleText||&apos;|&apos;) &gt; 2) then
