&#x9;&#x9;&#x9; *****************************************************************************/
&#x9;&#x9;&#x9;-- Parse the switches for the &apos;print_datasource_lineage&apos; option
&#x9;&#x9;&#x9;set startTimeDatasourceLineage = CURRENT_TIMESTAMP;
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/helpers/parseDocSwitches(switches, &apos;print_datasource_accessed&apos;, commandOptionValueDsAccessed);
&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/helpers/parseDocSwitches(switches, &apos;print_datasource_lineage&apos;, commandOptionValueDsLineage);
&#x9;&#x9;&#x9;if (commandOptionValueDsAccessed = 1 OR commandOptionValueDsLineage = 1) then
&#x9;&#x9;&#x9;&#x9;call /shared/Delta_DV/&quot;_scripts&quot;/Utilities/documentation/modules/getDocDataSourceLineage(
&#x9;&#x9;&#x9;&#x9;&#x9;r.&quot;path&quot;, r.&quot;type&quot;, constantPath, commandOptionValueDsAccessed, commandOptionValueDsLineage, moduleText);
&#x9;&#x9;&#x9;&#x9;if (moduleText is null) then
