PROCEDURE generateGuid(
&#x9;IN  generateUuidPath&#x9;&#x9;VARCHAR(4000),
&#x9;OUT result &#x9;&#x9;&#x9;&#x9;&#x9;LONGVARCHAR
)
BEGIN
&#x9;DECLARE version&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE sqlStatement&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE generateUuidPathMod&#x9;LONGVARCHAR;
&#x9;DECLARE curs&#x9;&#x9;&#x9;&#x9;CURSOR(guid LONGVARCHAR);

&#x9;CALL /shared/Delta_DV/&quot;_scripts&quot;/Utilities/repository/RepoUtils/applyReservedListToPath(generateUuidPath, null, generateUuidPathMod);
&#x9;SET sqlStatement = 
  &#x9;&apos;SELECT JT.* FROM&apos;||CHR(10)||
&#x9;&apos;    JSON_TABLE(&apos;||CHR(10)||
&#x9;&apos;        C.&quot;output&quot;,&apos;||CHR(10)||
&#x9;&apos;        &apos;&apos;$&apos;&apos;&apos;||CHR(10)||
&#x9;&apos;        COLUMNS(UUID VARCHAR(255) PATH &apos;&apos;$.generatedUUID&apos;&apos;)&apos;||CHR(10)||&#x9;  
&#x9;&apos;    ) JT,&apos;||CHR(10)||
  &#x9;generateUuidPathMod||&apos;() C&apos;;
&#x9;--CALL PRINT(&apos;sqlStatement=&apos;||sqlStatement);
&#x9;OPEN curs FOR sqlStatement;
&#x9;FETCH curs INTO result;
&#x9;CLOSE curs;
END</attribute>
