  <attribute name="Script" type="STRING">PROCEDURE prcBl_GetContractEligibility(
    IN inContractID VARCHAR(25), 
    OUT result PIPE (
&#x9;&#x9;EnrolleeID VARCHAR(27),
&#x9;&#x9;ContractID VARCHAR(25),
&#x9;&#x9;MemberID VARCHAR(2),
&#x9;&#x9;ContractEffDate&#x9;DATE,
&#x9;&#x9;ContractEndDate DATE,
&#x9;&#x9;ContractTermReasonCD VARCHAR(4),
&#x9;&#x9;ContractTermReasonDESC VARCHAR(100),
&#x9;&#x9;EligEffDate&#x9;DATE,
&#x9;&#x9;EligEndDate&#x9;DATE,
&#x9;&#x9;EligTermReasonCD VARCHAR(4),
&#x9;&#x9;EligTermReasonDESC VARCHAR(100),
&#x9;&#x9;ContractTypeCD VARCHAR(4),
&#x9;&#x9;ContractTypeDESC VARCHAR(100),
&#x9;&#x9;MBRClassCD VARCHAR(4),
&#x9;&#x9;MBRClassDESC VARCHAR(100),
&#x9;&#x9;GroupID VARCHAR(12),
&#x9;&#x9;DivisionID VARCHAR(12),
&#x9;&#x9;GroupDivision VARCHAR(25),
&#x9;&#x9;BenefitPkgID VARCHAR(8),
&#x9;&#x9;GroupType VARCHAR(12),
&#x9;&#x9;ContractHolder VARCHAR(12),
&#x9;&#x9;State VARCHAR(12),
&#x9;&#x9;Product VARCHAR(12),
&#x9;&#x9;ApplyWaitingPeriodIND VARCHAR(5)
       )
    )
    BEGIN

        DECLARE contract_not_exist EXCEPTION;
&#x9;&#x9;DECLARE unknown_errror EXCEPTION;
&#x9;&#x9;DECLARE bad_request EXCEPTION;
        DECLARE rowContractElig ROW (
&#x9;&#x9;EnrolleeID VARCHAR(27),
&#x9;&#x9;ContractID VARCHAR(25),
&#x9;&#x9;MemberID VARCHAR(2),
&#x9;&#x9;ContractEffDate&#x9;DATE,
&#x9;&#x9;ContractEndDate DATE,
&#x9;&#x9;ContractTermReasonCD VARCHAR(4),
&#x9;&#x9;ContractTermReasonDESC VARCHAR(100),
&#x9;&#x9;EligEffDate&#x9;DATE,
&#x9;&#x9;EligEndDate&#x9;DATE,
&#x9;&#x9;EligTermReasonCD VARCHAR(4),
&#x9;&#x9;EligTermReasonDESC VARCHAR(100),
&#x9;&#x9;ContractTypeCD VARCHAR(4),
&#x9;&#x9;ContractTypeDESC VARCHAR(100),
&#x9;&#x9;MBRClassCD VARCHAR(4),
&#x9;&#x9;MBRClassDESC VARCHAR(100),
&#x9;&#x9;GroupID VARCHAR(12),
&#x9;&#x9;DivisionID VARCHAR(12),
&#x9;&#x9;GroupDivision VARCHAR(25),
&#x9;&#x9;BenefitPkgID VARCHAR(8),
&#x9;&#x9;GroupType VARCHAR(12),
&#x9;&#x9;ContractHolder VARCHAR(12),
&#x9;&#x9;State VARCHAR(12),
&#x9;&#x9;Product VARCHAR(12),
&#x9;&#x9;ApplyWaitingPeriodIND VARCHAR(5)
        );
&#x9;&#x9;
&#x9;&#x9;DECLARE curContractElig CURSOR (
&#x9;&#x9;EnrolleeID VARCHAR(27),
&#x9;&#x9;ContractID VARCHAR(25),
&#x9;&#x9;MemberID VARCHAR(2),
&#x9;&#x9;ContractEffDate&#x9;DATE,
&#x9;&#x9;ContractEndDate DATE,
&#x9;&#x9;ContractTermReasonCD VARCHAR(4),
&#x9;&#x9;ContractTermReasonDESC VARCHAR(100),
&#x9;&#x9;EligEffDate&#x9;DATE,
&#x9;&#x9;EligEndDate&#x9;DATE,
&#x9;&#x9;EligTermReasonCD VARCHAR(4),
&#x9;&#x9;EligTermReasonDESC VARCHAR(100),
&#x9;&#x9;ContractTypeCD VARCHAR(4),
&#x9;&#x9;ContractTypeDESC VARCHAR(100),
&#x9;&#x9;MBRClassCD VARCHAR(4),
&#x9;&#x9;MBRClassDESC VARCHAR(100),
&#x9;&#x9;GroupID VARCHAR(12),
&#x9;&#x9;DivisionID VARCHAR(12),
&#x9;&#x9;GroupDivision VARCHAR(25),
&#x9;&#x9;BenefitPkgID VARCHAR(8),
&#x9;&#x9;GroupType VARCHAR(12),
&#x9;&#x9;ContractHolder VARCHAR(12),
&#x9;&#x9;State VARCHAR(12),
&#x9;&#x9;Product VARCHAR(12),
&#x9;&#x9;ApplyWaitingPeriodIND VARCHAR(5)
        );


&#x9;&#x9;IF inContractID = &apos;&apos; or inContractID IS NULL THEN  
         &#x9;RAISE bad_request VALUE &apos;Missing contractID in the request&apos;;
      &#x9;END IF;

&#x9;&#x9;CALL /shared/Delta_DV/Physical/Formatting/PackagedQueries/Enrollment/pkgPl_FamilyContractEligibility(inContractID, curContractElig);

      &#x9;contract_elig: BEGIN
         LOOP
            FETCH curContractElig INTO rowContractElig;
               IF NOT curContractElig.FOUND THEN
              &#x9;&#x9; LEAVE contract_elig;
               END IF;
               INSERT INTO result VALUES (rowContractElig.EnrolleeID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.MemberID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractEffDate,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractEndDate,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractTermReasonCD,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractTermReasonDESC,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.EligEffDate,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.EligEndDate,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.EligTermReasonCD,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.EligTermReasonDESC,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractTypeCD,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractTypeDESC,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.MBRClassCD,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.MBRClassDESC,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.GroupID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.DivisionID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.GroupDivision,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.BenefitPkgID,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.GroupType,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ContractHolder,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.State,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.Product,
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;rowContractElig.ApplyWaitingPeriodIND);
          END LOOP;
      &#x9;END; --contract_elig

 &#x9;&#x9;IF curContractElig.ROWCOUNT = 0 THEN  
         &#x9;RAISE contract_not_exist VALUE &apos;ContractID [&apos; || inContractID || &apos;] is not found&apos;;
      &#x9;END IF;

&#x9;&#x9;EXCEPTION
&#x9;&#x9;WHEN contract_not_exist THEN
&#x9;&#x9;&#x9;call /lib/util/SetWebSourceProperty(&apos;status&apos;,&apos;404&apos;,&apos;SERVICE.SETHTTPHEADER&apos;);
&#x9;&#x9;&#x9;RAISE VALUE SUBSTR(CURRENT_EXCEPTION.MESSAGE,1,500);
&#x9;&#x9;WHEN bad_request THEN
&#x9;&#x9;&#x9;call /lib/util/SetWebSourceProperty(&apos;status&apos;,&apos;400&apos;,&apos;SERVICE.SETHTTPHEADER&apos;);
&#x9;&#x9;&#x9;RAISE VALUE SUBSTR(CURRENT_EXCEPTION.MESSAGE,1,500);
&#x9;&#x9;ELSE
            IF LENGTH(CURRENT_EXCEPTION.MESSAGE)&gt;1 THEN
               RAISE VALUE SUBSTR(CURRENT_EXCEPTION.MESSAGE,1,500);
            ELSE
               RAISE VALUE SUBSTR(CURRENT_EXCEPTION.NAME,1,500);
            END IF;
    END

</attribute>
