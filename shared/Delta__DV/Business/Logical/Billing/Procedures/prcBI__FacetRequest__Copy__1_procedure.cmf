&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(REPLACE(REPLACE(rFacet.FacetField,&apos;[&apos;,&apos;&apos;),&apos;]&apos;,&apos;&apos;),&apos;&quot;&apos;,&apos;&apos;),
