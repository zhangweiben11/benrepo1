&#x9;&#x9;&#x9;&#x9;&#x9;jRow.InvoiceDateEnd,
&#x9;&#x9;&#x9;&#x9;&#x9;jRow.InvoiceNumber,
&#x9;&#x9;&#x9;&#x9;&#x9;jRow.InvoiceStatus,
&#x9;&#x9;&#x9;&#x9;&#x9;jRow.InvoiceType,
