  <attribute name="Script" type="STRING">PROCEDURE prcBI_FacetRequest_Copy_1(IN inSearchJson VARCHAR(50000), OUT outPaymentSearch PIPE(
&#x9;&#x9;SearchDetails VARCHAR(4000),
&#x9;&#x9;FacetField VARCHAR(500),&#x9;
