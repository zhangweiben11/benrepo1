&#x9;&#x9;&#x9;&#x9;&#x9;jRow.ReconLineItemExists);
&#x9;&#x9;SET ix = ix + 1;

&#x9;&#x9;FETCH NEXT FROM cFacet INTO rFacet;

&#x9;END;
&#x9;END WHILE;
&#x9;CLOSE cFacet;

&#x9;END IF;


&#x9;IF ix = 0
