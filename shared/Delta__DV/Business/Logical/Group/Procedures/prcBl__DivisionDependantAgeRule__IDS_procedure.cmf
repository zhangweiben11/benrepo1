  <attribute name="Script" type="STRING">PROCEDURE prcBl_DivisionDependantAgeRule_IDS(
    IN inGroupID VARCHAR(12), 
    IN inDivisionID VARCHAR(12), 
    IN inAsOfDate DATE,
    IN inGroupSurrogateIDChar VARCHAR(25), 
    IN inDivisionSurrogateIDChar VARCHAR(25), 
    OUT result PIPE (
        GroupID VARCHAR(12),
        DivisionID VARCHAR(12),
&#x9;    State VARCHAR(2),
&#x9;    DependantAge VARCHAR(2),
&#x9;    DependantAgeRule VARCHAR(50),
&#x9;    StudentAge VARCHAR(2),
&#x9;    StudentAgeRule VARCHAR(50),
&#x9;    EffDate DATE,
&#x9;    EndDate DATE
        )
    )
BEGIN
        DECLARE varEffDateZ2 DATE;
        DECLARE varEndDateZ2 DATE;

        DECLARE curDepAgeRule CURSOR (
        GroupID VARCHAR(12),
        DivisionID VARCHAR(12),
&#x9;    State VARCHAR(2),
&#x9;    FirstName VARCHAR(2),
&#x9;    MiddleName VARCHAR(2),
&#x9;    LastName VARCHAR(2),
&#x9;    ContactTitle VARCHAR(10),
&#x9;    NameSuffix VARCHAR(10),
&#x9;    EffDateZ2 DATE,
&#x9;    EndDateZ2 DATE,
&#x9;    DependantAgeRule VARCHAR(2),
&#x9;    StudentAgeRule VARCHAR(2),
&#x9;    EffDate DATE,
&#x9;    EndDate DATE
        );

        DECLARE rowDepAgeRule ROW (
        GroupID VARCHAR(12),
        DivisionID VARCHAR(12),
&#x9;    State VARCHAR(2),
&#x9;    FirstName VARCHAR(2),
&#x9;    MiddleName VARCHAR(2),
&#x9;    LastName VARCHAR(2),
&#x9;    ContactTitle VARCHAR(10),
&#x9;    NameSuffix VARCHAR(10),
&#x9;    EffDateZ2 DATE,
&#x9;    EndDateZ2 DATE,
&#x9;    DependantAgeRule VARCHAR(2),
&#x9;    StudentAgeRule VARCHAR(2),
&#x9;    EffDate DATE,
&#x9;    EndDate DATE
        );

        CALL /shared/Delta_DV/Physical/Formatting/PackagedQueries/&quot;Group&quot;/pkgPl_DivisionDependantAgeRule_IDS(inGroupID, inDivisionID, inAsOfDate, inGroupSurrogateIDChar, inDivisionSurrogateIDChar, curDepAgeRule);

        L_division: BEGIN
           LOOP
              FETCH curDepAgeRule INTO rowDepAgeRule;
                 IF NOT curDepAgeRule.FOUND THEN
                    LEAVE L_division;
                 END IF;
                 SET varEffDateZ2 = NVL(prcPl_DateStringToDate(rowDepAgeRule.ContactTitle),rowDepAgeRule.EffDateZ2);
                 SET varEndDateZ2 = NVL(prcPl_DateStringToDate(rowDepAgeRule.NameSuffix),rowDepAgeRule.EndDateZ2);

                 IF inAsOfDate &gt;= varEffDateZ2
                 AND inAsOfDate &lt;= varEndDateZ2
                 THEN
                    INSERT INTO result
                    SELECT rowDepAgeRule.GroupID, rowDepAgeRule.DivisionID, rowDepAgeRule.State, rowDepAgeRule.LastName, rowDepAgeRule.DependantAgeRule, rowDepAgeRule.FirstName, rowDepAgeRule.StudentAgeRule, 
                           GREATEST(varEffDateZ2, rowDepAgeRule.EffDate), 
                           LEAST (varEndDateZ2, rowDepAgeRule.EndDate);

                 END IF;
           END LOOP;
        END; --L_division
END</attribute>
